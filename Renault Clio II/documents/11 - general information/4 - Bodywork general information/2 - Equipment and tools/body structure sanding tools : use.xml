   <procedure id="d1e114" translate="no">
      <title translate="yes">sanding large surfaces</title>
      <procbody translate="no">
         <procsteps translate="no">
            <procstep translate="no">
               <para translate="yes">Use a large manual sanding block with or without extractor.</para>
            </procstep>
         </procsteps>
      </procbody>
      <procedure id="d1e123" translate="no">
         <title translate="yes">"Velcro" type sanding block with extractor for 115x226 mm, 100 or 150 grit strips</title>
         <procbody translate="no">
            <procsteps translate="no">
               <figure id="00-04-03-16-1-repair-00-04-03-16-01-79-1-1-1" translate="no">
                  
                  <image dimension="F1 (85x60) #" href="../MergedPictures/ILL-000028532_LAY-000001944.png" style="width: 85.0mm; height: 60.0mm;" translate="no"/><illusnum>RPR-000012436-04-03-16-1-repair-00-04-03-16-01-79-1-1-1-000028532</illusnum>
               </figure>
               <procstep translate="no">
                  <para translate="yes">Advantages:</para>
                  <ul translate="no">
                     <li translate="yes">allows the paper to be maintained in perfect contact with the block, preventing the paper from folding,</li>
                     <li translate="yes">worn paper is easily replaced,</li>
                     <li translate="yes">the extractor system avoids dust buildup.</li>
                  </ul>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Disadvantages:</para>
                  <ul translate="no">
                     <li translate="yes">suction pipe obstructs movement slightly.</li>
                  </ul>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Operator protection:</para>
                  <ul translate="no">
                     <li translate="yes">obligatory respiratory protection.</li>
                  </ul>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Protection in the work area:</para>
                  <ul translate="no">
                     <li translate="yes">None</li>
                  </ul>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e162" translate="no">
         <title translate="yes">"Kaupp" type sanding block</title>
         <procbody translate="no">
            <procsteps translate="no">
               <figure id="00-04-03-16-2-repair-00-04-03-16-01-79-1-1-2" translate="no">
                  
                  <image dimension="F1 (85x60) #" href="../MergedPictures/ILL-000028533_LAY-000001945.png" style="width: 85.0mm; height: 60.0mm;" translate="no"/><illusnum>RPR-000012436-04-03-16-2-repair-00-04-03-16-01-79-1-1-2-000028533</illusnum>
               </figure>
               <procstep translate="no">
                  <para translate="yes">Advantages:</para>
                  <ul translate="no">
                     <li translate="yes">is compatible with standard paper types,</li>
                     <li translate="yes">holds the paper during sanding.</li>
                  </ul>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Disadvantages:</para>
                  <ul translate="no">
                     <li translate="yes">paper must be cut to size,</li>
                     <li translate="yes">extraction not possible.</li>
                  </ul>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Operator protection:</para>
                  <ul translate="no">
                     <li translate="yes">obligatory respiratory protection.</li>
                  </ul>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Protection in the work area:</para>
                  <ul translate="no">
                     <li translate="yes">None</li>
                  </ul>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
   </procedure>
   <procedure id="d1e199" translate="no">
      <title translate="yes">angle and small area surfacing</title>
      <procbody translate="no">
         <procsteps translate="no">
            <procstep translate="no">
               <para translate="yes">Use a standard size manual sanding block with or without extractor.</para>
            </procstep>
         </procsteps>
      </procbody>
      <procedure id="d1e208" translate="no">
         <title translate="yes">"Velcro" type sanding block with extractor for 81 x 153 mm, 100 or 150 grit strips</title>
         <procbody translate="no">
            <procsteps translate="no">
               <figure id="00-04-03-16-3-repair-00-04-03-16-01-79-1-1-3" translate="no">
                  
                  <image dimension="F1 (85x60) #" href="../MergedPictures/ILL-000028534_LAY-000001946.png" style="width: 85.0mm; height: 60.0mm;" translate="no"/><illusnum>RPR-000012436-04-03-16-3-repair-00-04-03-16-01-79-1-1-3-000028534</illusnum>
               </figure>
               <procstep translate="no">
                  <para translate="yes">Advantages:</para>
                  <ul translate="no">
                     <li translate="yes">is compatible with orbital sander paper,</li>
                     <li translate="yes">allows the paper to be maintained in perfect contact with the block, preventing the paper from folding,</li>
                     <li translate="yes">worn paper is easily replaced,</li>
                     <li translate="yes">allows angles to be sanded,</li>
                     <li translate="yes">the extractor system avoids dust buildup.</li>
                  </ul>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Disadvantages:</para>
                  <ul translate="no">
                     <li translate="yes">suction pipe obstructs movement slightly.</li>
                  </ul>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Operator protection:</para>
                  <ul translate="no">
                     <li translate="yes">obligatory respiratory protection.</li>
                  </ul>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Protection in the work area:</para>
                  <ul translate="no">
                     <li translate="yes">None</li>
                  </ul>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e251" translate="no">
         <title translate="yes">"Block" type sanding block</title>
         <procbody translate="no">
