<?xml version="1.0" encoding="UTF-8" standalone="no"?><procedure xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" appl="X35|X65|X76" id="RPR-000292040" xml:lang="en" xsi:noNamespaceSchemaLocation="urn:ptc:names:renault:dita:xsd:procedure.xsd"><title translate="yes">front seat belt buckle : removal - refitting</title><procprolog translate="no"><status name="" translate="no"/><critdates translate="no"><created date="" translate="no"/><revised modified="" translate="no"/></critdates><procmetadata translate="no"><siename translate="no"/><sieversion translate="no"/><sietype translate="yes">Procedure</sietype><sienumber translate="no"/><domain translate="no"/><company translate="no">RENAULT</company></procmetadata></procprolog><procbody translate="no"><introduction translate="no"><noteR translate="no" type="warning"><para translate="yes">To avoid all risk of damage to the systems, apply the safety and cleanliness instructions and operation recommendations before carrying out any repair:</para><ul translate="no"><li translate="yes"><sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3539306"><linkdesc>airbag and pretensioners : precautions for the repair</linkdesc><linkvalue>GIEMR-000004</linkvalue></sieref></li><li translate="yes"><sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3503040"><linkdesc>vehicle : precautions for the repair</linkdesc><linkvalue>GIEMR-002446</linkvalue></sieref></li></ul></noteR></introduction></procbody><procedure id="d1e21" otherprops="WF-SET='01,34'" translate="no"><title translate="yes">removal</title><procbody translate="no"/><procedure appl="X35/CEE1,CEI1|X65|X76" id="d1e25" translate="no"><title translate="yes">removal preparation operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Switch off the ignition.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Disconnect the battery<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3511330"><linkdesc>12 V battery : removal - refitting</linkdesc><linkvalue>GIEMR-000248</linkvalue></sieref>.</para></procstep></procsteps></procbody></procedure><procedure id="d1e45" translate="no"><title translate="yes">removal preparation operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Remove the central console<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3458808"><linkdesc>centre console assembly : removal - refitting</linkdesc><linkvalue>GIEMR-000782</linkvalue></sieref>.</para></procstep></procsteps><procsteps appl="X35|X65" translate="no"><procstep translate="no"><para translate="yes">Remove the front seat lower casing<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3506157"><linkdesc>front seat lower casing : removal - refitting</linkdesc><linkvalue>GIEMR-000331</linkvalue></sieref>.</para></procstep></procsteps></procbody></procedure><procedure appl="X35" id="d1e68" translate="no"><title translate="yes">removal operation</title><procbody translate="no"><procsteps translate="no"><marklayer id="marklayer-d1e73" translate="no"><mark id="MARK.REPAIR-31X02X01X19-01X37-1-6.1" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">1</designation></mark></marklayer><figure id="31-02-01-19-8-repair-31-02-01-19-01-37-1-6-4" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000042990_LAY-000073788.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Remove:</para><ul translate="no"><li translate="yes">the bolt<markref format="dita" href="#d1e68/31-02-01-19-8-repair-31-02-01-19-01-37-1-6-4" scope="local" translate="no" type="figure">1</markref>with its washer,</li><li translate="yes">the front seat belt buckle.</li></ul></procstep></procsteps></procbody></procedure><procedure appl="X35/CEE1,CEI1|X65|X76" id="d1e95" translate="no"><title translate="yes">removal operation</title><procbody translate="no"><procsteps translate="no"><marklayer id="marklayer-d1e107" translate="no"><mark id="MARK.REPAIR-31X02X01X19-01X37-1-6.2" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">2</designation></mark><mark id="MARK.REPAIR-31X02X01X19-01X37-1-6.3" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">3</designation></mark></marklayer><figure id="31-02-01-19-10-repair-31-02-01-19-01-37-1-6-5" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000042992_LAY-000073789.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Disconnect the font seat belt buckle connector<markref format="dita" href="#d1e95/31-02-01-19-10-repair-31-02-01-19-01-37-1-6-5" scope="local" translate="no" type="figure">2</markref>.</para></procstep><procstep translate="no"><para translate="yes">Remove:</para><ul translate="no"><li translate="yes">the bolt<markref format="dita" href="#d1e95/31-02-01-19-10-repair-31-02-01-19-01-37-1-6-5" scope="local" translate="no" type="figure">3</markref>,</li><li translate="yes">the front seat belt buckle.</li></ul></procstep></procsteps></procbody></procedure></procedure><procedure id="d1e142" otherprops="WF-SET='01,61'" translate="no"><title translate="yes">refitting</title><procbody translate="no"/><procedure id="d1e146" translate="no"><title translate="yes">refitting operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Coat the threading of the bolt of the front seat belt buckle with<ingredientname translate="yes">HIGH STRENGTH THREAD LOCK</ingredientname><sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3542379"><linkdesc>vehicle : parts and consumables for the repair</linkdesc><linkvalue>GIEMR-002445</linkvalue></sieref>(04B, Consumables - Products).</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Refit the front seat belt buckle.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Torque tighten the<torque translate="no"><torquedobject translate="yes">front seat belt buckle bolt</torquedobject><technicalvalue translate="no"><value translate="yes">25 N.m</value></technicalvalue></torque>.</para></procstep></procsteps></procbody></procedure><procedure appl="X35/CEE1,CEI1|X65|X76" id="d1e169" translate="no"><title translate="yes">refitting operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Connect the front seat belt buckle connector.</para></procstep></procsteps></procbody></procedure><procedure id="d1e186" translate="no"><title translate="yes">final operation</title><procbody translate="no"><procsteps appl="X35|X65" translate="no"><procstep translate="no"><para translate="yes">Refit the front lower casing<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3506157"><linkdesc>front seat lower casing : removal - refitting</linkdesc><linkvalue>GIEMR-000331</linkvalue></sieref></para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Refit the central console<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3458808"><linkdesc>centre console assembly : removal - refitting</linkdesc><linkvalue>GIEMR-000782</linkvalue></sieref>.</para></procstep></procsteps></procbody></procedure><procedure appl="X35/CEE1,CEI1|X65|X76" id="d1e205" translate="no"><title translate="yes">final operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Connect the battery<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3511330"><linkdesc>12 V battery : removal - refitting</linkdesc><linkvalue>GIEMR-000248</linkvalue></sieref>.</para><noteR translate="no" type="warning"><para translate="yes">To eliminate the risk of any accident, do not reuse pyrotechnic components.</para><para translate="yes">It is essential to destroy pretensioners or airbags before scrapping a vehicle or a part only<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3449308"><linkdesc>airbag and pretensioners : recycling - destruction</linkdesc><linkvalue>GIEMR-000005</linkvalue></sieref>.</para></noteR></procstep></procsteps></procbody></procedure></procedure></procedure>