 translate="no"><procsteps appl="X35/ABPA01|X65" translate="no"><procstep translate="no"><para translate="yes">Lock the airbag computer using the<!--Arbortext, Inc., 1988-2015, v.4002--><?Pub Inc?><materialname otherprops="MAT09_EN" translate="yes" xml:lang="en" xsi:noNamespaceSchemaLocation="urn:ptc:names:renault:dita:xsd:materialname.xsd">Diagnostic tool</materialname>(see Fault finding - Replacement of components ) (88C, Airbags and pretensioners).</para></procstep><procstep translate="no"><para translate="yes">Disconnect the battery<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3511330"><linkdesc>12 V battery : removal - refitting</linkdesc><linkvalue>GIEMR-000248</linkvalue></sieref>(80A, Battery).</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Remove:</para><ul translate="no"><li translate="yes">the front side door seals partially,</li><li translate="yes">the windscreen pillar trims<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3506195"><linkdesc>windscreen pillar lining : removal - refitting</linkdesc><linkvalue>GIEMR-001291</linkvalue></sieref>(71A, Body internal trim).</li></ul></procstep><procstep translate="no"><para translate="yes">Open the glovebox cover.</para></procstep><procstep translate="no"><para translate="yes">Adjust the steering wheel to the lowest position.</para></procstep></procsteps><procsteps translate="no"><marklayer id="marklayer-d1e81" translate="no"><mark id="MARK.REPAIR-50X08X02X20-01X37-1-2.1" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">1</designation></mark></marklayer><figure id="31-01-02-04-5-repair-50-08-02-20-01-37-1-2-8" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000035420_LAY-000098455.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Remove:</para><ul translate="no"><li translate="yes">the bolts<markref format="dita" href="#d1e37/31-01-02-04-5-repair-50-08-02-20-01-37-1-2-8" scope="local" translate="no" type="figure">1</markref>,</li><li translate="yes">the lower cover.</li></ul></procstep></procsteps><procsteps translate="no"><figure id="31-01-02-04-6-repair-50-08-02-20-01-37-1-2-11" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000035421_LAY-000098451.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Unclip the upper cover.</para></procstep></procsteps><procsteps translate="no"><marklayer id="marklayer-d1e114" translate="no"><mark id="MARK.REPAIR-50X08X02X20-01X37-1-2.2" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">2</designation></mark></marklayer><figure id="50-08-02-20-5-repair-50-08-02-20-01-37-1-2-9" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000040032_LAY-000098456.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><figure id="50-08-02-20-4-repair-50-08-02-20-01-37-1-2-2" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000040031_LAY-000098452.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Unclip the trims at<markref format="dita" href="#d1e37/50-08-02-20-5-repair-50-08-02-20-01-37-1-2-9" scope="local" translate="no" type="figure">2</markref>using the tool<toolref format="dita" href="x-wc://file=OS-000001526.xml" scope="external" translate="no" type="specifictool"><linkdesc>Set of trim removal levers.</linkdesc><linkvalue>Car. 1363</linkvalue></toolref>.</para></procstep></procsteps></procbody></procedure><procedure id="d1e143" translate="no"><title translate="yes">removal operation</title><procbody translate="no"><procsteps translate="no"><marklayer id="marklayer-d1e145" translate="no"><mark id="MARK.REPAIR-50X08X02X20-01X37-1-2.3" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">3</designation></mark></marklayer><figure id="50-08-02-20-3-repair-50-08-02-20-01-37-1-2-10" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000040030_LAY-000098450.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Remove the clips<markref format="dita" href="#d1e143/50-08-02-20-3-repair-50-08-02-20-01-37-1-2-10" scope="local" translate="no" type="figure">3</markref>from each side.</para></procstep></procsteps><procsteps translate="no"><marklayer id="marklayer-d1e163" translate="no"><mark id="MARK.REPAIR-50X08X02X20-01X37-1-2.4" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">4</designation></mark></marklayer><figure id="50-08-02-20-1-repair-50-08-02-20-01-37-1-2-3" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000040028_LAY-000098453.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><figure id="50-08-02-20-2-repair-50-08-02-20-01-37-1-2-5" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000040029_LAY-000098454.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Remove:</para><ul translate="no"><li translate="yes">the bolts<markref format="dita" href="#d1e143/50-08-02-20-1-repair-50-08-02-20-01-37-1-2-3" scope="local" translate="no" type="figure">4</markref>,</li><li translate="yes">the dashboard upper section.</li></ul></procstep></procsteps></procbody></procedure></procedure><procedure id="d1e201" otherprops="WF-SET='01,61'" translate="no"><title translate="yes">refitting</title><procbody translate="no"/><procedure id="d1e205" translate="no"><title translate="yes">refitting operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Refit:</para><ul translate="no"><li translate="yes">the dashboard upper section,</li><li translate="yes">the bolts<markref format="dita" href="#d1e143/50-08-02-20-1-repair-50-08-02-20-01-37-1-2-3" scope="local" translate="no" type="figure">4</markref>,</li><li translate="yes">the clips<markref format="dita" href="#d1e143/50-08-02-20-3-repair-50-08-02-20-01-37-1-2-10" scope="local" translate="no" type="figure">3</markref>.</li></ul></procstep></procsteps></procbody></procedure><procedure id="d1e225" translate="no"><title translate="yes">final operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Refit:</para><ul translate="no"><li translate="yes">the trims,</li><li translate="yes">the cowlings under the steering wheel,</li><li translate="yes">the bolts<markref format="dita" href="#d1e37/31-01-02-04-5-repair-50-08-02-20-01-37-1-2-8" scope="local" translate="no" type="figure">1</markref>.</li></ul></procstep><procstep translate="no"><para translate="yes">Close the glovebox cover.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Refit:</para><ul translate="no"><li translate="yes">the windscreen pillar trims<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3506195"><linkdesc>windscreen pillar lining : removal - refitting</linkdesc><linkvalue>GIEMR-001291</linkvalue></sieref>(71A, Body internal trim),</li><li translate="yes">the front side door seals.</li></ul></procstep></procsteps><procsteps appl="X35/ABPA01|X65" translate="no"><procstep translate="no"><para translate="yes">Connect the battery<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3511330"><linkdesc>12 V battery : removal - refitting</linkdesc><linkvalue>GIEMR-000248</linkvalue></sieref>(80A, Battery).</para></procstep><procstep translate="no"><para translate="yes">Unlock the airbag computer using the<!--Arbortext, Inc., 1988-2015, v.4002--><?Pub Inc?><materialname otherprops="MAT09_EN" translate="yes" xml:lang="en" xsi:no