<?xml version="1.0" encoding="UTF-8" standalone="no"?><procedure xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" appl="X65" id="RPR-000398587" xml:lang="en" xsi:noNamespaceSchemaLocation="urn:ptc:names:renault:dita:xsd:procedure.xsd"><title translate="yes">headlining : removal - refitting</title><procprolog translate="no"><status name="" translate="no"/><critdates translate="no"><created date="" translate="no"/><revised modified="" translate="no"/></critdates><procmetadata translate="no"><siename translate="no"/><sieversion translate="no"/><sietype translate="yes">Procedure</sietype><sienumber translate="no"/><domain translate="no"/><company translate="no">RENAULT</company></procmetadata></procprolog><procbody translate="no"/><procedure id="d1e6" otherprops="WF-SET='01,34'" translate="no"><title translate="yes">removal</title><procbody translate="no"/><procedure id="d1e10" translate="no"><title translate="yes">removal preparation operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Remove:</para><ul translate="no"><li translate="yes">the sun visors<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3493420"><linkdesc>sun visor : removal - refitting</linkdesc><linkvalue>GIEMR-001703</linkvalue></sieref>(57A, Interior equipment),</li><li translate="yes">the interior light<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3451689"><linkdesc>interior lights : removal - refitting</linkdesc><linkvalue>GIEMR-001765</linkvalue></sieref>(81B, Interior lighting),</li><li translate="yes">the grab handles<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3480289"><linkdesc>grab handle : removal - refitting</linkdesc><linkvalue>GIEMR-001639</linkvalue></sieref>(57A, Interior equipment),</li><li translate="yes">the front side door seals<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3516322"><linkdesc>front side opening element frame seal : removal - refitting</linkdesc><linkvalue>GIEMR-001349</linkvalue></sieref>.</li></ul></procstep></procsteps><procsteps appl="X65/B65" translate="no"><procstep translate="no"><para translate="yes">Remove:</para><ul translate="no"><li translate="yes">the rear side door seals<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3469655"><linkdesc>rear side opening element fixed window seal : removal - refitting</linkdesc><linkvalue>GIEMR-001348</linkvalue></sieref>,</li><li translate="yes">the front door sill linings<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3446632"><linkdesc>door sill trim : removal - refitting</linkdesc><linkvalue>GIEMR-001275</linkvalue></sieref>,</li><li translate="yes">the rear door sill linings<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3446752"><linkdesc>rear door sill lining : removal - refitting</linkdesc><linkvalue>GIEMR-001276</linkvalue></sieref>,</li><li translate="yes">the B-pillar linings<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3510129"><linkdesc>B-pillar lining : removal - refitting</linkdesc><linkvalue>GIEMR-001121</linkvalue></sieref>.</li></ul></procstep></procsteps><procsteps appl="X65/C65" translate="no"><procstep translate="no"><para translate="yes">Remove:</para><ul translate="no"><li translate="yes">the 1/3 and 2/3 rear bench seat base<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3503288"><linkdesc>rear bench seat base : removal - refitting</linkdesc><linkvalue>GIEMR-000186</linkvalue></sieref>,</li><li translate="yes">the 1/3 and 2/3 rear bench seatbacks<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3477516"><linkdesc>rear bench seatback : removal - refitting</linkdesc><linkvalue>GIEMR-000806</linkvalue></sieref>,</li><li translate="yes">the rear speakers (if fitted)<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3526086"><linkdesc>rear speaker : removal - refitting</linkdesc><linkvalue>GIEMR-001436</linkvalue></sieref>,</li><li translate="yes">the rear panel trims<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3456863"><linkdesc>rear side panel trim : removal - refitting</linkdesc><linkvalue>GIEMR-001292</linkvalue></sieref>,</li><li translate="yes">the B-pillar upper trims<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3526417"><linkdesc>B-pillar upper trim : removal - refitting</linkdesc><linkvalue>GIEMR-001138</linkvalue></sieref>.</li></ul></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Remove:</para><ul translate="no"><li translate="yes">the rear quarter panel trims<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3527269"><linkdesc>rear quarter panel lining : removal - refitting</linkdesc><linkvalue>GIEMR-001287</linkvalue></sieref>,</li><li translate="yes">the windscreen pillar trims<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3506195"><linkdesc>windscreen pillar lining : removal - refitting</linkdesc><linkvalue>GIEMR-001291</linkvalue></sieref>.</li></ul></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Tilt the seatbacks of the front seats backwards.</para></procstep></procsteps></procbody></procedure><procedure id="d1e124" translate="no"><title translate="yes">removal operation</title><procbody translate="no"><procsteps translate="no"><marklayer id="marklayer-d1e126" translate="no"><mark id="MARK.REPAIR-70X02X04X02-01X37-1-27.1" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">1</designation></mark></marklayer><figure id="70-02-04-02-73-repair-70-02-04-02-01-37-1-27-1" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000041612_LAY-000097912.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Remove the clip<markref format="dita" href="#d1e124/70-02-04-02-73-repair-70-02-04-02-01-37-1-27-1" scope="local" translate="no" type="figure">1</markref>using the tool<toolref format="dita" href="x-wc://file=OS-000001526.xml" scope="external" translate="no" type="specifictool"><linkdesc>Set of trim removal levers.</linkdesc><linkvalue>Car. 1363</linkvalue></toolref>.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Remove the mounting supports of the sun visors by:</para><ul translate="no"><li translate="yes">removing the support bolt,</li><li translate="yes">tightening the support bolt by a few turns,</li><li translate="yes">pushing on the support bolt to put the bush in the "not deformed" position,</li><li translate="yes">unclipping the supports.</li></ul></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Move aside the headlining.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Cut the cement bead.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Remove the headlining (this operation requires two people).</para></procstep></procsteps></procbody></procedure></procedure><procedure id="d1e167" otherprops="WF-SET='01,61'" translate="no"><title translate="yes">refitting</title><procbody translate="no"/><procedure id="d1e171" translate="no"><title translate="yes">refitting preparation operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Level the residual cement bead on the roof.</para></procstep><procstep translate="no"><para translate="yes">Carefully remove the residual cement bead on the headlining.</para></procstep></procsteps><procsteps translate="no"><marklayer id="marklayer-d1e181" translate="no"><mark id="MARK.REPAIR-70X02X04X02-01X37-1-27.2" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">2</designation></mark></marklayer><figure id="70-02-04-02-72-repair-70-02-04-02-01-37-1-27-2" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000041611_LAY-000097913.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Apply<ingredientname translate="yes">MONOPAC EVOLUTION ADHESIVE KIT</ingredientname><markref format="dita" href="#d1e171/70-02-04-02-72-repair-70-02-04-02-01-37-1-27-2" scope="local" translate="no" type="figure">2</markref>to the headlining<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3542379"><linkdesc>vehicle : parts and consumables for the repair</linkdesc><linkvalue>GIEMR-002445</linkvalue></sieref>.</para></procstep></procsteps></procbody></procedure><procedure id="d1e208" translate="no"><title translate="yes">refitting operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Position the headlining.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Refit the indexing clip.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Bond the headlining (this operation requires two people).</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Refit the mounting supports of the sun visors.</para></procstep></procsteps></procbody></procedure><procedure id="d1e224" translate="no"><title translate="yes">final operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Proceed in the reverse order to removal.</para></procstep></procsteps></procbody></procedure></procedure></procedure>