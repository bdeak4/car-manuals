   <procedure id="d1e91" otherprops="WF-SET='01,34'" translate="no">
      <title translate="yes">removal</title>
      <procbody translate="no"/>
      <procedure id="d1e95" translate="no">
         <title translate="yes">engine repair preparation operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <noteR translate="no" type="warning">
                     <para translate="yes">To avoid all risk of damage to the systems, apply the safety and cleanliness instructions and operation recommendations before carrying out any repair<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3455841">
                           <linkdesc>engine : precautions for the repair</linkdesc>
                           <linkvalue>GIEMR-001545</linkvalue>
                        </sieref>.</para>
                  </noteR>
                  <noteR translate="no" type="caution">
                     <para translate="yes">It is strictly forbidden to apply any pressure to the engine oil sump. Damage to the sump may also damage the engine by:</para>
                     <ul translate="no">
                        <li translate="yes">blocking the strainer,</li>
                        <li translate="yes">raising the oil level above maximum, with a risk of engine racing.</li>
                     </ul>
                  </noteR>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove the engine - gearbox assembly<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3457628">
                        <linkdesc>engine and gearbox assembly : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-001096</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps appl="X33/K4M/BVM5|X35/K4J,K4M/BVM5|X38/K4M/BVM5,BVM6|X44/K4M/BVM5|X52/K4M/BVM5|X56/K4M/BVM5|X61/K4M/BVM5|X64/K4J,K4M/BVM5|X65/K4J,K4M/BVM5|X74/K4M/BVM5,BVM6|X76/K4M/APL03/BVM5|X77/K4J,K4M/BVM5|X79/K4M/BVM5,BVM6|X84/K4J,K4M/BVM5,BVM6|X85/K4J,K4M/BVM5,BVM6|X90/K4M/BVM5|X91/K4M/BVM6|X95/K4M/BVM5,BVM6" translate="no">
               <procstep translate="no">
                  <para translate="yes">Disconnect the manual gearbox from the engine<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3461102">
                        <linkdesc>gearbox assembly : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-000921</linkvalue>
                     </sieref>(21A, Manual gearbox).</para>
               </procstep>
            </procsteps>
            <procsteps appl="X35/K4J,K4M/BVA4|X38/K4M/BVA4|X56/K4M/BVA4|X61/K4M/BVA4|X64/K4J,K4M/BVA4|X65/K4J,K4M/BVA4|X74/K4M/BVA4,BVA5|X76/K4M/APL03/BVA4|X77/K4J,K4M/BVA4|X79/K4M/BVA4|X84/K4J,K4M/BVA4|X85/K4J,K4M/BVA4|X90/K4M/BVA4|X92/K4M/BVA4" translate="no">
               <procstep translate="no">
                  <para translate="yes">Disconnect the automatic gearbox from the engine<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3461102">
                        <linkdesc>gearbox assembly : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-000921</linkvalue>
                     </sieref>(23A, Automatic transmission).</para>
               </procstep>
            </procsteps>
            <procsteps appl="X33/K4M|X35/K4J,K4M|X38/K4M|X44/K4M|X56/K4M|X61/K4M|X64/K4J,K4M|X65/K4J,K4M|X74/K4M|X76/K4M/APL03|X77/K4J,K4M|X79/K4M|X84/K4J,K4M|X85/K4J,K4M|X90/K4M|X91/K4M|X95/K4M" translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove the accessories belt<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3498977">
                        <linkdesc>accessories belt : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-000720</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps appl="X52/K4M|X92/K4M" translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove the accessories belt<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3498977">
                        <linkdesc>accessories belt : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-000720</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <figure id="10-03-03-06-15-repair-10-01-01-22-1-6-1" translate="no">
                  
                  <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000004433_LAY-000006552.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/><illusnum>RPR-000015835-03-03-06-15-repair-10-01-01-22-1-6-1-000004433</illusnum>
               </figure>
               <procstep translate="no">
                  <para translate="yes">Fit the flywheel locking tool<toolref format="dita" href="x-wc://file=OS-000002349.xml" scope="external" translate="no" type="specifictool">
                        <linkdesc>Flywheel locking tool.</linkdesc>
                        <linkvalue>Mot. 1677</linkvalue>
                     </toolref>onto the cylinder block.</para>
               </procstep>
            </procsteps>
            <procsteps appl="X33/K4M|X35/K4J,K4M|X38/K4M|X44/K4M|X56/K4M|X61/K4M|X64/K4J,K4M|X65/K4J,K4M|X74/K4M|X76/K4M/APL03|X77/K4J,K4M|X79/K4M|X84/K4J,K4M|X85/K4J,K4M|X90/K4M|X91/K4M|X95/K4M" translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove the crankshaft accessories pulley<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3481018">
                        <linkdesc>crankshaft accessories pulley : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-001655</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps appl="X52/K4M|X92/K4M" translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove the crankshaft accessories pulley<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3498977">
                        <linkdesc>accessories belt : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-000720</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps appl="X33/K4M/BVM5|X35/K4J,K4M/BVM5|X38/K4M/BVM5,BVM6|X44/K4M/BVM5|X52/K4M/BVM5|X56/K4M/BVM5|X61/K4M/BVM5|X64/K4J,K4M/BVM5|X65/K4J,K4M/BVM5|X74/K4M/BVM5,BVM6|X76/K4M/APL03/BVM5|X77/K4J,K4M/BVM5|X79/K4M/BVM5,BVM6|X84/K4J,K4M/BVM5,BVM6|X85/K4J,K4M/BVM5,BVM6|X90/K4M/BVM5|X91/K4M/BVM6|X95/K4M/BVM5,BVM6" translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove:</para>
                  <ul translate="no">
                     <li translate="yes">the clutch disc and pressure plate<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3470380">
                           <linkdesc>clutch plate : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000661</linkvalue>
                        </sieref>(20A, Clutch),</li>
                     <li translate="yes">the flywheel<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3499029">
                           <linkdesc>flywheel : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-002431</linkvalue>
                        </sieref>.</li>
                  </ul>
               </procstep>
            </procsteps>
            <procsteps appl="X35/K4J,K4M/BVA4|X38/K4M/BVA4|X52/K4M/BVA4|X56/K4M/BVA4|X61/K4M/BVA4|X64/K4J,K4M/BVA4|X65/K4J,K4M/BVA4|X74/K4M/BVA4,BVA5|X76/K4M/APL03/BVA4|X77/K4J,K4M/BVA4|X79/K4M/BVA4|X84/K4J,K4M/BVA4|X85/K4J,K4M/BVA4|X90/K4M/BVA4|X92/K4M/BVA4|X92/K4M/BVA4" translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove the drive plate<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3488096">
                        <linkdesc>drive plate : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-002387</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove the flywheel locking tool<toolref format="dita" href="x-wc://file=OS-000002349.xml" scope="external" translate="no" type="specifictool">
                        <linkdesc>Flywheel locking tool.</linkdesc>
                        <linkvalue>Mot. 1677</linkvalue>
                     </toolref>.</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Place the engine on the component support<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3454399">
                        <linkdesc>component support equipment : use</linkdesc>
                        <linkvalue>GIEMR-001522</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Drain the engine<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3457199">
                        <linkdesc>engine oil : draining - refilling</linkdesc>
                        <linkvalue>GIEMR-001296</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e588" translate="no">
         <title translate="yes">upper engine removal operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove:</para>
                  <ul translate="no">
                     <li translate="yes">the timing belt<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3499206">
                           <linkdesc>timing belt : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000726</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the coolant pump<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3492401">
                           <linkdesc>water pump : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001682</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the alternator<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3451714">
                           <linkdesc>alternator : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000049</linkvalue>
                        </sieref>.</li>
                  </ul>
               </procstep>
            </procsteps>
            <procsteps appl="X33/K4M/CA|X35/K4J,K4M/CA|X38/K4M/CA|X44/K4M/CA|X52/K4M/CA01,CA02|X56/K4M/CA|X61/K4M/CA|X64/K4J,K4M/CA|X65/K4J,K4M/CA|X74/K4M/CA|X76/K4M/APL03/CA|X77/K4J,K4M/CA|X79/K4M/CA|X84/K4J,K4M/CA|X85/K4J,K4M/CA|X90/K4M/CA|X91/K4M|X92/K4M/CA|X95/K4M/CA" translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove the air conditioning compressor<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3475245">
                        <linkdesc>air conditioning compressor : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-000616</linkvalue>
                     </sieref>(62A, Air conditioning).</para>
               </procstep>
            </procsteps>
            <procsteps appl="X33/K4M|X35/K4J,K4M|X38/K4M|X44/K4M/DA|X52/K4M/DA|X56/K4M|X61/K4M|X64/K4J,K4M|X65/K4J,K4M|X74/K4M|X76/K4M/APL03|X77/K4J,K4M|X79/K4M|X84/K4J,K4M|X85/K4J,K4M|X90/K4M/DA|X91/K4M|X95/K4M" translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove the power-assisted steering pump<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3491958">
                        <linkdesc>power-assisted steering pump : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-001674</linkvalue>
                     </sieref>(36B, Power-assisted steering).</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove:</para>
                  <ul translate="no">
                     <li translate="yes">the multifunction support<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3524290">
                           <linkdesc>multifunction support : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001996</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the inlet distributor<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3542753">
                           <linkdesc>intake distributor : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-002040</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the oil decanter<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3478905">
                           <linkdesc>oil decanter : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000833</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the ignition coils<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3511828">
                           <linkdesc>ignition coil : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000270</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the exhaust manifold<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3541478">
                           <linkdesc>exhaust manifold : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000563</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the water chamber<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3502792">
                           <linkdesc>coolant outlet unit : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000129</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the injector holder shim<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3529762">
                           <linkdesc>injector holder shim : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000434</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the rocker cover<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3456065">
                           <linkdesc>rocker cover : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000738</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the camshafts<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3456415">
                           <linkdesc>cylinder head : stripping - rebuilding</linkdesc>
                           <linkvalue>GIEMR-000743</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the cylinder head<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3456300">
                           <linkdesc>cylinder head : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000741</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the valves<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3472599">
                           <linkdesc>valve : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-002101</linkvalue>
                        </sieref>.</li>
                  </ul>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e808" translate="no">
         <title translate="yes">upper engine repair operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Clean the cylinder head<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3456535">
                        <linkdesc>cylinder head : cleaning</linkdesc>
                        <linkvalue>GIEMR-000744</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Check:</para>
                  <ul translate="no">
                     <li translate="yes">the cylinder head and the rocker cover<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3517632">
                           <linkdesc>cylinder head : check</linkdesc>
                           <linkvalue>GIEMR-000739</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the camshafts<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3475160">
                           <linkdesc>camshaft : check</linkdesc>
                           <linkvalue>GIEMR-000142</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the valves and the valve springs<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3472480">
                           <linkdesc>valve : check</linkdesc>
                           <linkvalue>GIEMR-002100</linkvalue>
                        </sieref>.</li>
                  </ul>
                  <noteR translate="no" type="note">
                     <para translate="yes">Note:</para>
                     <para translate="yes">Replace the parts whose values are outside the permitted tolerance values.</para>
                  </noteR>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e842" translate="no">
         <title translate="yes">lower engine removal operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove:</para>
                  <ul translate="no">
                     <li translate="yes">the oil pressure sensor<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3475578">
                           <linkdesc>engine oil pressure sensor : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000342</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the oil filter<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3492903">
                           <linkdesc>oil filter : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001243</linkvalue>
                        </sieref>
                        <sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3457199">
                           <linkdesc>engine oil : draining - refilling</linkdesc>
                           <linkvalue>GIEMR-001296</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the coolant pump inlet pipe<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3529116">
                           <linkdesc>coolant pump inlet pipe : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-002288</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the crankshaft seal on the timing side<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3458831">
                           <linkdesc>crankshaft seal, timing end : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001325</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the gearbox side crankshaft seal<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3523264">
                           <linkdesc>crankshaft seal, gearbox side : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001324</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the sump<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3530961">
                           <linkdesc>engine oil sump : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000475</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the oil pump<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3492706">
                           <linkdesc>engine oil pump : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001687</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the "piston - con rod" assembly of every cylinder<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3513206">
                           <linkdesc>piston - con rod : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001762</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the crankshaft<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3541251">
                           <linkdesc>crankshaft : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-002404</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the cylinder block<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3531062">
                           <linkdesc>cylinder block : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000479</linkvalue>
                        </sieref>.</li>
                  </ul>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e902" translate="no">
         <title translate="yes">lower engine repair operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Clean the cylinder block<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3448166">
                        <linkdesc>cylinder block : cleaning</linkdesc>
                        <linkvalue>GIEMR-000481</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Check:</para>
                  <ul translate="no">
                     <li translate="yes">the cylinder block<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3447941">
                           <linkdesc>cylinder block : check</linkdesc>
                           <linkvalue>GIEMR-000478</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the "con rod - piston" assemblies<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3451443">
                           <linkdesc>piston - con rod : check</linkdesc>
                           <linkvalue>GIEMR-001761</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the crankshaft<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3497833">
                           <linkdesc>crankshaft : check</linkdesc>
                           <linkvalue>GIEMR-002403</linkvalue>
                        </sieref>.</li>
                  </ul>
                  <noteR translate="no" type="note">
                     <para translate="yes">Note:</para>
                     <para translate="yes">Replace the parts whose values are outside the permitted tolerance values.</para>
                  </noteR>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
   </procedure>
   <procedure id="d1e936" otherprops="WF-SET='01,61'" translate="no">
      <title translate="yes">refitting</title>
      <procbody translate="no"/>
      <procedure id="d1e940" translate="no">
         <title translate="yes">refitting preparation operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Parts always to be replaced:</para>
                  <ul translate="no">
                     <li translate="yes">all seals,</li>
                     <li translate="yes">the cooling circuit hoses if they are damaged,</li>
                     <li translate="yes">the accessories and timing belts,</li>
                     <li translate="yes">the timing and accessories tensioners and rollers,</li>
                     <li translate="yes">the tensioning roller bolts,</li>
                     <li translate="yes">the crankshaft accessories pulley bolt,</li>
                     <li translate="yes">the exhaust manifold nuts,</li>
                     <li translate="yes">the exhaust manifold studs (if removed),</li>
                     <li translate="yes">the caps on the bottom of the cylinder head (flywheel end),</li>
                     <li translate="yes">the inlet camshaft dephaser cap,</li>
                     <li translate="yes">the camshaft pulley nuts,</li>
                     <li translate="yes">the inlet camshaft dephaser bolt,</li>
                     <li translate="yes">the piston base cooling jets,</li>
                     <li translate="yes">the oil filter,</li>
                     <li translate="yes">the flywheel bolts,</li>
                     <li translate="yes">the drive plate bolts,</li>
                     <li translate="yes">the con rod cap bolts,</li>
                     <li translate="yes">the crankshaft bearing cap bolts,</li>
                     <li translate="yes">the valve guides,</li>
                     <li translate="yes">the cylinder head bolts.</li>
                  </ul>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <noteR translate="no" type="caution">
                     <para translate="yes">The gasket faces must be clean, dry and free from grease (avoid finger marks)</para>
                  </noteR>
                  <noteR translate="no" type="caution">
                     <para translate="yes">Applying excess sealant could cause it to be squeezed out when parts are tightened. A mixture of sealant and fluid could damage certain components (engine, radiator, etc.).</para>
                  </noteR>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e996" translate="no">
         <title translate="yes">lower engine refitting operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit the cylinder block<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3531062">
                        <linkdesc>cylinder block : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-000479</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">When replacing the cylinder block, crankshaft or crankshaft bearing shells, always determine the thickness class for each bearing shell that will be fitted on each crankshaft bearing, to ensure that journal clearance is within tolerances<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3497833">
                        <linkdesc>crankshaft : check</linkdesc>
                        <linkvalue>GIEMR-002403</linkvalue>
                     </sieref>.</para>
                  <noteR translate="no" type="note">
                     <para translate="yes">Note:</para>
                     <para translate="yes">Any clearances of the crankshaft journal outside the tolerances can cause engine damage.</para>
                  </noteR>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit the crankshaft<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3541251">
                        <linkdesc>crankshaft : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-002404</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">If the cylinder block or pistons are replaced, it is essential to check the pairing between the category of piston and that of the cylinder block barrel<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3447941">
                        <linkdesc>cylinder block : check</linkdesc>
                        <linkvalue>GIEMR-000478</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit:</para>
                  <ul translate="no">
                     <li translate="yes">the "piston - con rod" assembly of every cylinder<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3513206">
                           <linkdesc>piston - con rod : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001762</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the oil pump<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3492706">
                           <linkdesc>engine oil pump : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001687</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the sump<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3530961">
                           <linkdesc>engine oil sump : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000475</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the gearbox side crankshaft seal<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3523264">
                           <linkdesc>crankshaft seal, gearbox side : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001324</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the crankshaft seal on the timing side<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3458831">
                           <linkdesc>crankshaft seal, timing end : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001325</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the coolant pump inlet pipe<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3529116">
                           <linkdesc>coolant pump inlet pipe : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-002288</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the oil filter<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3492903">
                           <linkdesc>oil filter : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001243</linkvalue>
                        </sieref>
                        <sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3457199">
                           <linkdesc>engine oil : draining - refilling</linkdesc>
                           <linkvalue>GIEMR-001296</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the oil pressure sensor<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3475578">
                           <linkdesc>engine oil pressure sensor : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000342</linkvalue>
                        </sieref>.</li>
                  </ul>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e1075" translate="no">
         <title translate="yes">upper engine refitting operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit:</para>
                  <ul translate="no">
                     <li translate="yes">the valves<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3472599">
                           <linkdesc>valve : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-002101</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the cylinder head<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3456300">
                           <linkdesc>cylinder head : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000741</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the camshafts<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3456415">
                           <linkdesc>cylinder head : stripping - rebuilding</linkdesc>
                           <linkvalue>GIEMR-000743</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the rocker cover<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3456065">
                           <linkdesc>rocker cover : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000738</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the injector holder shim<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3529762">
                           <linkdesc>injector holder shim : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000434</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the water chamber<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3502792">
                           <linkdesc>coolant outlet unit : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000129</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the exhaust manifold<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3541478">
                           <linkdesc>exhaust manifold : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000563</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the ignition coils<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3511828">
                           <linkdesc>ignition coil : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000270</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the oil decanter<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3478905">
                           <linkdesc>oil decanter : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000833</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the inlet distributor<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3542753">
                           <linkdesc>intake distributor : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-002040</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the multifunction support<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3524290">
                           <linkdesc>multifunction support : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001996</linkvalue>
                        </sieref>.</li>
                  </ul>
               </procstep>
            </procsteps>
            <procsteps appl="X33/K4M|X35/K4J,K4M|X38/K4M|X44/K4M/DA|X52/K4M/DA|X56/K4M|X61/K4M|X64/K4J,K4M|X65/K4J,K4M|X74/K4M|X76/K4M/APL03|X77/K4J,K4M|X79/K4M|X84/K4J,K4M|X85/K4J,K4M|X90/K4M/DA|X91/K4M|X95/K4M" translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit the power-assisted steering pump<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3491958">
                        <linkdesc>power-assisted steering pump : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-001674</linkvalue>
                     </sieref>(36B, Power-assisted steering).</para>
               </procstep>
            </procsteps>
            <procsteps appl="X33/K4M/CA|X35/K4J,K4M/CA|X38/K4M/CA|X44/K4M/CA|X52/K4M/CA01,CA02|X56/K4M/CA|X61/K4M/CA|X64/K4J,K4M/CA|X65/K4J,K4M/CA|X74/K4M/CA|X76/K4M/APL03/CA|X77/K4J,K4M/CA|X79/K4M/CA|X84/K4J,K4M/CA|X85/K4J,K4M/CA|X90/K4M/CA|X91/K4M|X92/K4M/CA|X95/K4M/CA" translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit the air conditioning compressor<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3475245">
                        <linkdesc>air conditioning compressor : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-000616</linkvalue>
                     </sieref>(62A, Air conditioning).</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit:</para>
                  <ul translate="no">
                     <li translate="yes">the alternator<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3451714">
                           <linkdesc>alternator : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000049</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the coolant pump<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3492401">
                           <linkdesc>water pump : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001682</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the timing belt<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3499206">
                           <linkdesc>timing belt : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000726</linkvalue>
                        </sieref>.</li>
                  </ul>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e1295" translate="no">
         <title translate="yes">final operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove the engine from the component support<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3454399">
                        <linkdesc>component support equipment : use</linkdesc>
                        <linkvalue>GIEMR-001522</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps appl="X33/K4M/BVM5|X35/K4J,K4M/BVM5|X38/K4M/BVM5,BVM6|X44/K4M/BVM5|X52/K4M/BVM5|X56/K4M/BVM5|X61/K4M/BVM5|X64/K4J,K4M/BVM5|X65/K4J,K4M/BVM5|X74/K4M/BVM5,BVM6|X76/K4M/APL03/BVM5|X77/K4J,K4M/BVM5,BVM6|X79/K4M/BVM5,BVM6|X84/K4J,K4M/BVM5|X85/K4J,K4M/BVM5,BVM6|X90/K4M/BVM5|X91/K4M/BVM6|X95/K4M/BVM5,BVM6" translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit:</para>
                  <ul translate="no">
                     <li translate="yes">the clutch disc and pressure plate<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3470380">
                           <linkdesc>clutch plate : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000661</linkvalue>
                        </sieref>(20A, Clutch),</li>
                     <li translate="yes">the flywheel<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3499029">
                           <linkdesc>flywheel : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-002431</linkvalue>
                        </sieref>.</li>
                  </ul>
               </procstep>
            </procsteps>
            <procsteps appl="X35/K4J,K4M/BVA4|X38/K4M/BVA4|X52/K4M/BVA4|X56/K4M/BVA4|X61/K4M/BVA4|X64/K4J,K4M/BVA4|X65/K4J,K4M/BVA4|X74/K4M/BVA4,BVA5|X76/K4M/APL03/BVA4|X77/K4J,K4M/BVA4|X79/K4M/BVA4|X84/K4J,K4M/BVA4|X85/K4J,K4M/BVA4|X90/K4M/BVA4|X92/K4M/BVA4" translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit the drive plate<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3488096">
                        <linkdesc>drive plate : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-002387</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps appl="X33/K4M|X35/K4J,K4M|X38/K4M|X44/K4M|X56/K4M|X61/K4M|X64/K4J,K4M|X65/K4J,K4M|X74/K4M|X76/K4M/APL03|X77/K4J,K4M|X79/K4M|X84/K4J,K4M|X85/K4J,K4M|X90/K4M|X91/K4M|X95/K4M" translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit:</para>
                  <ul translate="no">
                     <li translate="yes">the flywheel locking tool<toolref format="dita" href="x-wc://file=OS-000002349.xml" scope="external" translate="no" type="specifictool">
                           <linkdesc>Flywheel locking tool.</linkdesc>
                           <linkvalue>Mot. 1677</linkvalue>
                        </toolref>on the cylinder block,</li>
                     <li translate="yes">the crankshaft accessories pulley<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3481018">
                           <linkdesc>crankshaft accessories pulley : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001655</linkvalue>
                        </sieref>.</li>
                  </ul>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Remove the flywheel locking tool<toolref format="dita" href="x-wc://file=OS-000002349.xml" scope="external" translate="no" type="specifictool">
                        <linkdesc>Flywheel locking tool.</linkdesc>
                        <linkvalue>Mot. 1677</linkvalue>
                     </toolref>from the cylinder block.</para>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Refit the accessories belt<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3498977">
                        <linkdesc>accessories belt : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-000720</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps appl="X52/K4M|X92/K4M" translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit:</para>
                  <ul translate="no">
                     <li translate="yes">the flywheel locking tool<toolref format="dita" href="x-wc://file=OS-000002349.xml" scope="external" translate="no" type="specifictool">
                           <linkdesc>Flywheel locking tool.</linkdesc>
                           <linkvalue>Mot. 1677</linkvalue>
                        </toolref>on the cylinder block,</li>
                     <li translate="yes">the crankshaft accessories pulley<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3498977">
                           <linkdesc>accessories belt : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000720</linkvalue>
                        </sieref>.</li>
                  </ul>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Remove the flywheel locking tool<toolref format="dita" href="x-wc://file=OS-000002349.xml" scope="external" translate="no" type="specifictool">
                        <linkdesc>Flywheel locking tool.</linkdesc>
                        <linkvalue>Mot. 1677</linkvalue>
                     </toolref>from the cylinder block.</para>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Refit the accessories belt<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3498977">
                        <linkdesc>accessories belt : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-000720</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps appl="X33/K4M/BVM5|X35/K4J,K4M/BVM5|X38/K4M/BVM5,BVM6|X44/K4M/BVM5|X52/K4M/BVM5|X56/K4M/BVM5|X61/K4M/BVM5|X64/K4J,K4M/BVM5|X65/K4J,K4M/BVM5|X74/K4M/BVM5,BVM6|X76/K4M/APL03/BVM5|X77/K4J,K4M/BVM5|X79/K4M/BVM5,BVM6|X84/K4J,K4M/BVM5,BVM6|X85/K4J,K4M/BVM5,BVM6|X90/K4M/BVM5|X91/K4M/BVM6|X95/K4M/BVM5,BVM6" translate="no">
               <procstep translate="no">
                  <para translate="yes">Connect the manual gearbox to the engine<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3461102">
                        <linkdesc>gearbox assembly : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-000921</linkvalue>
                     </sieref>(21A, Manual gearbox).</para>
               </procstep>
            </procsteps>
            <procsteps appl="X35/K4J,K4M/BVA4|X38/K4M/BVA4|X52/K4M/BVA4|X56/K4M/BVA4|X61/K4M/BVA4|X64/K4J,K4M/BVA4|X65/K4J,K4M/BVA4|X74/K4M/BVA4,BVA5|X76/K4M/APL03/BVA4|X77/K4J,K4M/BVA4|X79/K4M/BVA4|X84/K4J,K4M/BVA4|X85/K4J,K4M/BVA4|X90/K4M/BVA4|X92/K4M/BVA4" translate="no">
               <procstep translate="no">
                  <para translate="yes">Connect the automatic gearbox to the engine<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3461102">
                        <linkdesc>gearbox assembly : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-000921</linkvalue>
                     </sieref>(23A, Automatic transmission).</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit the engine - gearbox assembly<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3457628">
                        <linkdesc>engine and gearbox assembly : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-001096</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps appl="X33/K4M|X35/K4J,K4M|X38/K4M|X44/K4M|X56/K4M|X61/K4M|X64/K4J,K4M|X65/K4J,K4M|X74/K4M|X76/K4M/APL03|X77/K4J,K4M|X79/K4M|X84/K4J,K4M|X85/K4J,K4M|X90/K4M|X91/K4M|X95/K4M" translate="no">
               <procstep translate="no">
                  <para translate="yes">Top up the engine oil<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3457199">
                        <linkdesc>engine oil : draining - refilling</linkdesc>
                        <linkvalue>GIEMR-001296</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
            <procsteps appl="X52/K4M|X92/K4M" translate="no">
               <procstep translate="no">
                  <para translate="yes">Top up the engine oil<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3457199">
                        <linkdesc>engine oil : draining - refilling</linkdesc>
                        <linkvalue>GIEMR-001296</linkvalue>
                     </sieref>.</para>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
   </procedure>
</procedure>