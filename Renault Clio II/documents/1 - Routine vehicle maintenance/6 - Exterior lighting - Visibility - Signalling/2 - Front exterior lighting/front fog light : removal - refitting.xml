<?xml version="1.0" encoding="UTF-8" standalone="no"?><procedure xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" appl="X65" id="RPR-000372403" xml:lang="en" xsi:noNamespaceSchemaLocation="urn:ptc:names:renault:dita:xsd:procedure.xsd"><title translate="yes">front fog light : removal - refitting</title><procprolog translate="no"><status name="" translate="no"/><critdates translate="no"><created date="" translate="no"/><revised modified="" translate="no"/></critdates><procmetadata translate="no"><siename translate="no"/><sieversion translate="no"/><sietype translate="yes">Procedure</sietype><sienumber translate="no"/><domain translate="no"/><company translate="no">RENAULT</company></procmetadata></procprolog><procbody translate="no"/><procedure id="d1e6" otherprops="WF-SET='01,34'" translate="no"><title translate="yes">removal</title><procbody translate="no"/><procedure id="d1e10" translate="no"><title translate="yes">removal preparation operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Position the vehicle on a two-post lift<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3500084"><linkdesc>vehicle : towing and lifting</linkdesc><linkvalue>GIEMR-002447</linkvalue></sieref>(02A, Lifting equipment).</para></procstep></procsteps></procbody></procedure><procedure id="d1e20" translate="no"><title translate="yes">removal operation</title><procbody translate="no"><procsteps translate="no"><marklayer id="marklayer-d1e22" translate="no"><mark id="MARK.REPAIR-80X02X02X06-01X37-1-26.1" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">2</designation></mark><mark id="MARK.REPAIR-80X02X02X06-01X37-1-26.2" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">1</designation></mark></marklayer><figure id="80-02-02-07-13-repair-80-02-02-06-01-37-1-26-1" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000045017_LAY-000102587.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Unclip the front wheel arch liner at<markref format="dita" href="#d1e20/80-02-02-07-13-repair-80-02-02-06-01-37-1-26-1" scope="local" translate="no" type="figure">1</markref>.</para></procstep><procstep translate="no"><para translate="yes">Move the front wheel arch liner to one side.</para></procstep><procstep translate="no"><para translate="yes">Disconnect the front fog light connector<markref format="dita" href="#d1e20/80-02-02-07-13-repair-80-02-02-06-01-37-1-26-1" scope="local" translate="no" type="figure">2</markref>.</para></procstep></procsteps><procsteps translate="no"><marklayer id="marklayer-d1e55" translate="no"><mark id="MARK.REPAIR-80X02X02X06-01X37-1-26.3" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">3</designation></mark></marklayer><figure id="80-02-02-06-34-repair-80-02-02-06-01-37-1-26-2" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000053852_LAY-000102588.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Remove:</para><ul translate="no"><li translate="yes">the bolts<markref format="dita" href="#d1e20/80-02-02-06-34-repair-80-02-02-06-01-37-1-26-2" scope="local" translate="no" type="figure">3</markref>from the front fog light,</li><li translate="yes">the front fog light.</li></ul></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">If replacing the front fog light bulb, remove the bulb<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3468866"><linkdesc>fog light bulb : removal - refitting</linkdesc><linkvalue>GIEMR-001333</linkvalue></sieref>.</para></procstep></procsteps></procbody></procedure></procedure><procedure id="d1e84" otherprops="WF-SET='01,61'" translate="no"><title translate="yes">refitting</title><procbody translate="no"/><procedure id="d1e88" translate="no"><title translate="yes">refitting preparation operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">If replacing the front fog light bulb, refit the bulb<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3468866"><linkdesc>fog light bulb : removal - refitting</linkdesc><linkvalue>GIEMR-001333</linkvalue></sieref>.</para></procstep></procsteps></procbody></procedure><procedure id="d1e98" translate="no"><title translate="yes">refitting operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Proceed in the reverse order to removal.</para></procstep></procsteps></procbody></procedure></procedure></procedure>