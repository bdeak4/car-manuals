<procedure id="d1e6" otherprops="WF-SET='01,34'" translate="no">
<title translate="yes">removal</title>
<procbody translate="no"/>
<procedure id="d1e10" translate="no">
<title translate="yes">removal preparation operation</title>
<procbody translate="no">
<procsteps translate="no">
<procstep translate="no"><para translate="yes">Open the boot.</para>
</procstep>
</procsteps>
</procbody>
</procedure>
<procedure id="d1e17" translate="no">
<title translate="yes">removal operation</title>
<procbody translate="no">
<procsteps translate="no"><marklayer id="marklayer-d1e19" translate="no"><?Pub Dtl?>
<mark id="MARK.REPAIR-80X02X06X08-01X37-1-39.1" translate="no">
<mcomponent translate="no"><label translate="no"/></mcomponent>
<designation translate="no">1</designation></mark></marklayer>
<figure id="80-02-06-08-6-repair-80-02-06-08-01-37-1-39-1" translate="no">
<image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000012134_LAY-000101609.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure>
<procstep translate="no"><para translate="yes">Remove the six bolts<markref format="dita" href="#d1e17/80-02-06-08-6-repair-80-02-06-08-01-37-1-39-1" scope="local" translate="no" type="figure">1</markref>of the upper
strip.</para></procstep>
</procsteps>
<procsteps translate="no"><marklayer id="marklayer-d1e41" translate="no"><?Pub Dtl?>
<mark id="MARK.REPAIR-80X02X06X08-01X37-1-39.2" translate="no">
<mcomponent translate="no"><label translate="no"/></mcomponent>
<designation translate="no">4</designation></mark><mark id="MARK.REPAIR-80X02X06X08-01X37-1-39.3" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">2</designation></mark><mark id="MARK.REPAIR-80X02X06X08-01X37-1-39.4" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">3</designation></mark></marklayer>
<figure id="80-02-06-08-5-repair-80-02-06-08-01-37-1-39-2" translate="no">
<image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000012133_LAY-000101610.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure>
<procstep translate="no"><para translate="yes">Unclip the upper strip
at<markref format="dita" href="#d1e17/80-02-06-08-5-repair-80-02-06-08-01-37-1-39-2" scope="local" translate="no" type="figure">2</markref>).</para>
</procstep>
<procstep translate="no"><para translate="yes">Disconnect the high
level brake light connector<markref format="dita" href="#d1e17/80-02-06-08-5-repair-80-02-06-08-01-37-1-39-2" scope="local" translate="no" type="figure">3</markref>.</para>
</procstep>
<procstep translate="no"><para translate="yes">Remove:</para>
<ul translate="no">
<li translate="yes">the high level brake light bolts<markref format="dita" href="#d1e17/80-02-06-08-5-repair-80-02-06-08-01-37-1-39-2" scope="local" translate="no" type="figure">4</markref>,</li>
<li translate="yes">the high level brake light.</li>
</ul>
<noteR translate="no" type="note">
<para translate="yes">Note:</para>
<para translate="yes">The bulbs cannot be removed.</para>
<para translate="yes">In the event of a fault, replace the complete
high-level brake light.</para>
</noteR>
</procstep>
</procsteps>
</procbody>
</procedure>
<procedure appl="X65/PE2012" id="d1e98" translate="no">
<title translate="yes">removal operation</title><?Pub Caret -1?>
<procbody translate="no">
<procsteps translate="no"><marklayer id="marklayer-d1e105" translate="no"><?Pub Dtl?><mark id="MARK.REPAIR-80X02X06X08-01X37-1-39.8" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">5</designation></mark></marklayer>
<figure id="80-02-06-64-repair-80-02-06-08-01-37-1-39-9" translate="no">

<image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000031710_LAY-000101611.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure>
<procstep translate="no"><para translate="yes">Remove the blanking
covers<markref format="dita" href="#d1e98/80-02-06-64-repair-80-02-06-08-01-37-1-39-9" scope="local" translate="no" type="figure">5</markref>.</para></procstep>
</procsteps>
<procsteps translate="no"><marklayer id="marklayer-d1e123" translate="no"><?Pub Dtl?><mark id="MARK.REPAIR-80X02X06X08-01X37-1-39.9" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">6</designation></mark></marklayer>
<figure id="80-02-06-65-repair-80-02-06-08-01-37-1-39-13" translate="no">

<image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000031711_LAY-000101607.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure>
<figure id="80-02-06-117-repair-80-02-06-08-01-37-1-39-10" translate="no">
<image dimension="F1 (85x60) #" href="../MergedPictures/ILL-000055623_LAY-000101605.png" style="width: 85.0mm; height: 60.0mm;" translate="no"/></figure>
<procstep translate="no"><para translate="yes">Unclip the high level
brake light with a flat-blade screwdriver by pressing on the clips<markref format="dita" href="#d1e98/80-02-06-117-repair-80-02-06-08-01-37-1-39-10" scope="local" translate="no" type="figure">6</markref>.</para>
</procstep>
</procsteps>
<procsteps translate="no"><marklayer id="marklayer-d1e143" translate="no"><?Pub Dtl?><mark id="MARK.REPAIR-80X02X06X08-01X37-1-39.10" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">9</designation></mark><mark id="MARK.REPAIR-80X02X06X08-01X37-1-39.11" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">8</designation></mark><mark id="MARK.REPAIR-80X02X06X08-01X37-1-39.12" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">7</designation></mark></marklayer>
<figure id="80-02-06-08-70-repair-80-02-06-08-01-37-1-39-14" translate="no">
<image dimension="F1 (85x60) #" href="../MergedPictures/ILL-000058490_LAY-000101608.png" style="width: 85.0mm; height: 60.0mm;" translate="no"/></figure>
<procstep translate="no"><para translate="yes">Disconnect:</para>
<ul translate="no">
<li translate="yes">the connector<markref format="dita" href="#d1e98/80-02-06-08-70-repair-80-02-06-08-01-37-1-39-14" scope="local" translate="no" type="figure">7</markref>from the high
level brake light,</li>
<li translate="yes">the supply pipe<markref format="dita" href="#d1e98/80-02-06-08-70-repair-80-02-06-08-01-37-1-39-14" scope="local" translate="no" type="figure">8</markref>from the rear
screen washer jet.</li>
</ul>
</procstep>
<procstep translate="no"><para translate="yes">Remove the high level
brake light<markref format="dita" href="#d1e98/80-02-06-08-70-repair-80-02-06-08-01-37-1-39-14" scope="local" translate="no" type="figure">9</markref>.</para>
</procstep>
</procsteps>
<procsteps translate="no">
<figure id="80-02-06-68-repair-80-02-06-08-01-37-1-39-12" translate="no">

<image dimension="F1 (85x60) #" href="../MergedPictures/ILL-000031714_LAY-000101606.png" style="width: 85.0mm; height: 60.0mm;" translate="no"/></figure>
<procstep translate="no"><para translate="yes">Remove:</para>
<ul translate="no">
<li translate="yes">the high level brake light bulb<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3469084"><linkdesc>rear light bulb : removal - refitting</linkdesc><linkvalue>GIEMR-001336</linkvalue>
</sieref>.</li>
</ul>
</procstep>
</procsteps>
</procbody>
</procedure>
</procedure>
<procedure id="d1e200" otherprops="WF-SET='01,61'" translate="no">
<title translate="yes">refitting</title>
<procbody translate="no"/>
<procedure id="d1e204" translate="no">
<title translate="yes">refitting operation</title>
<procbody translate="no">
<procsteps translate="no">
<procstep translate="no"><para translate="yes">Proceed in the reverse
order to removal.</para></procstep>
</procsteps>
</procbody>
</procedure>
<procedure appl="X65/PE2012" id="d1e210" translate="no">
<title translate="yes">refitting operation</title>
<procbody translate="no">
<procsteps translate="no">
<procstep translate="no"><para translate="yes">Refit:</para>
<ul translate="no">
<li translate="yes">the high level brake light bulb<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3469084"><linkdesc>rear light bulb : removal - refitting</linkdesc><linkvalue>GIEMR-001336</linkvalue>
</sieref>,</li>
</ul>
</procstep>
</procsteps>
<procsteps translate="no">
<procstep translate="no"><para translate="yes">Connect:</para>
<ul translate="no">
<li translate="yes">the rear screen washer jet supply pipe,</li>
<li translate="yes">the high level brake light connector.</li>
</ul>
</procstep>
</procsteps>
<procsteps translate="no">
<procstep translate="no"><para translate="yes">Refit:</para>
<ul translate="no">
<li translate="yes">the high level brake light,</li>
<li translate="yes">the blanking covers.</li>
</ul>
</procstep>
</procsteps>
</procbody>
</procedure>
</procedure>
</procedure>