<?xml version="1.0" encoding="UTF-8" standalone="no"?><procedure xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" appl="X65|X76/APL03|X84" id="RPR-000291075" xml:lang="en" xsi:noNamespaceSchemaLocation="urn:ptc:names:renault:dita:xsd:procedure.xsd">
   <title translate="yes">alternator pulley : removal-refitting</title>
   
   
   <procprolog translate="no">
      
      
      <status name="" translate="no"/>
      <critdates translate="no">
         <created date="" translate="no"/>
         <revised modified="" translate="no"/>
      </critdates>
      <procmetadata translate="no">
         <siename translate="no"/>
         <sieversion translate="no"/>
         <sietype translate="yes">Procedure</sietype>
         <sienumber translate="no"/>
         <domain translate="no"/>
         <company translate="no">RENAULT</company>
         
      </procmetadata>
   </procprolog>
   <procbody translate="no"/>
   <procedure id="d1e64" otherprops="WF-SET='01,37'" translate="no">
      <title translate="yes">parts to be replaced systematically</title>
      <procbody translate="no">
         <procsteps translate="no">
            <procstep translate="no">
               <ul translate="no">
                  <li translate="yes">Alternator belt,</li>
                  <li translate="yes">Tensioning roller,</li>
                  <li translate="yes">The tension wheel mounting bolts.</li>
               </ul>
            </procstep>
         </procsteps>
      </procbody>
   </procedure>
   <procedure id="d1e76" otherprops="WF-SET='01,37'" translate="no">
      <title translate="yes">equipment required</title>
      <procbody translate="no">
         <procsteps translate="no">
            <procstep translate="no">
               <ul translate="no">
                  <li translate="yes">A torque wrench that can tighten in an anticlockwise direction,</li>
                  <li translate="yes">Pneumatic wrench.</li>
               </ul>
            </procstep>
         </procsteps>
      </procbody>
   </procedure>
   <procedure id="d1e83" otherprops="WF-SET='01,37'" translate="no">
      <title translate="yes">equipment required</title>
      <procbody translate="no">
         <procsteps translate="no">
            <procstep translate="no">
               <noteR translate="no" type="note">
                  <para translate="yes">Note:</para>
                  <para translate="yes">The procedure for replacing the decoupling alternator pulley with a freewheel alternator pulley only applies to versions without air conditioning.</para>
               </noteR>
            </procstep>
         </procsteps>
      </procbody>
   </procedure>
   <procedure id="d1e91" otherprops="WF-SET='01,40'" translate="no">
      <title translate="yes">disassembly</title>
      <procbody translate="no">
         <procsteps translate="no">
            <procstep translate="no">
               <para translate="yes">Remove the vehicle alternator<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3451714">
                     <linkdesc>alternator : removal - refitting</linkdesc>
                     <linkvalue>GIEMR-000049</linkvalue>
                  </sieref>.</para>
            </procstep>
         </procsteps>
         <procsteps translate="no">
            <figure id="10-02-117-repair-32-02-01-01-01-43-1-1-1" translate="no">
               
               <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000018288_LAY-000072759.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
            </figure>
            <procstep translate="no">
               <para translate="yes">Place the alternator in a vice jaw.</para>
            </procstep>
         </procsteps>
         <procsteps translate="no">
            <marklayer id="marklayer-d1e106" translate="no">
               <mark id="MARK.REPAIR-32X02X01X01-01X43-1-1.1" translate="no">
                  <mcomponent translate="no">
                     <label translate="no"/>
                  </mcomponent>
                  <designation translate="no">1</designation>
               </mark>
            </marklayer>
            <figure id="10-02-127-repair-32-02-01-01-01-43-1-1-2" translate="no">
               
               <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000018468_LAY-000072770.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
            </figure>
            <figure id="10-02-118-repair-32-02-01-01-01-43-1-1-21" translate="no">
               
               <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000018289_LAY-000072761.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
            </figure>
            <procstep translate="no">
               <para translate="yes">Position the hexagon socket<markref format="dita" href="#d1e91/10-02-118-repair-32-02-01-01-01-43-1-1-21" scope="local" translate="no" type="figure">1</markref>from the tool box<toolref format="dita" href="x-wc://file=OS-000017226.xml" scope="external" translate="no" type="specifictool">
                     <linkdesc>Kit for replacing the alternator idler pulleys.</linkdesc>
                     <linkvalue>Mot. 1732</linkvalue>
                  </toolref>in the alternator pulley.</para>
            </procstep>
         </procsteps>
         <procsteps translate="no">
            <figure id="10-02-358-repair-32-02-01-01-01-43-1-1-20" translate="no">
               
               <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000021411_LAY-000072760.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
            </figure>
            <procstep translate="no">
               <para translate="yes">Remove the alternator pulley using a pneumatic wrench.</para>
            </procstep>
         </procsteps>
         <procsteps translate="no">
            <marklayer id="marklayer-d1e135" translate="no">
               <mark id="MARK.REPAIR-32X02X01X01-01X43-1-1.2" translate="no">
                  <mcomponent translate="no">
                     <label translate="no"/>
                  </mcomponent>
                  <designation translate="no">11</designation>
               </mark>
            </marklayer>
            <figure id="10-02-154-repair-32-02-01-01-01-43-1-1-22" translate="no">
               
               <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000015471_LAY-000072762.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
            </figure>
            <procstep translate="no">
               <para translate="yes">Remove the washer<markref format="dita" href="#d1e91/10-02-154-repair-32-02-01-01-01-43-1-1-22" scope="local" translate="no" type="figure">11</markref>which is<technicalvalue translate="no">
                     <value translate="yes">5.54 mm</value>
                  </technicalvalue>thick.</para>
            </procstep>
         </procsteps>
      </procbody>
   </procedure>
   <procedure id="d1e159" otherprops="WF-SET='01,55'" translate="no">
      <title translate="yes">reassembly</title>
      <procbody translate="no">
         <procsteps translate="no">
            <marklayer id="marklayer-d1e161" translate="no">
               <mark id="MARK.REPAIR-32X02X01X01-01X43-1-1.3" translate="no">
                  <mcomponent translate="no">
                     <label translate="no"/>
                  </mcomponent>
                  <designation translate="no">12</designation>
               </mark>
            </marklayer>
            <figure id="10-02-152-repair-32-02-01-01-01-43-1-1-3" translate="no">
               
               <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000015469_LAY-000072771.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
            </figure>
            <figure id="10-02-153-repair-32-02-01-01-01-43-1-1-23" translate="no">
               
               <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000015470_LAY-000072763.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
            </figure>
            <procstep translate="no">
               <para translate="yes">Refit the washer<markref format="dita" href="#d1e159/10-02-152-repair-32-02-01-01-01-43-1-1-3" scope="local" translate="no" type="figure">12</markref>(<technicalvalue translate="no">
                     <value translate="yes">Yellow</value>
                  </technicalvalue>) which can be found in the free wheel pulley kit and which is<technicalvalue translate="no">
                     <value translate="yes">4.18 mm</value>
                  </technicalvalue>thick, to the alternator rotor.</para>
            </procstep>
         </procsteps>
         <procsteps translate="no">
            <marklayer id="marklayer-d1e197" translate="no">
               <mark id="MARK.REPAIR-32X02X01X01-01X43-1-1.4" translate="no">
                  <mcomponent translate="no">
                     <label translate="no"/>
                  </mcomponent>
                  <designation translate="no">7</designation>
               </mark>
            </marklayer>
            <figure id="10-02-122-repair-32-02-01-01-01-43-1-1-24" translate="no">
               
               <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000018463_LAY-000072764.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
            </figure>
            <procstep translate="no">
               <para translate="yes">Screw free wheel pulley<markref format="dita" href="#d1e159/10-02-122-repair-32-02-01-01-01-43-1-1-24" scope="local" translate="no" type="figure">7</markref>onto the rotor.</para>
            </procstep>
         </procsteps>
         <procsteps translate="no">
            <marklayer id="marklayer-d1e214" translate="no">
               <mark id="MARK.REPAIR-32X02X01X01-01X43-1-1.5" translate="no">
                  <mcomponent translate="no">
                     <label translate="no"/>
                  </mcomponent>
                  <designation translate="no">7</designation>
               </mark>
               <mark id="MARK.REPAIR-32X02X01X01-01X43-1-1.6" translate="no">
                  <mcomponent translate="no">
                     <label translate="no"/>
                  </mcomponent>
                  <designation translate="no">8</designation>
               </mark>
            </marklayer>
            <figure id="10-02-123-repair-32-02-01-01-01-43-1-1-25" translate="no">
               
               <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000018464_LAY-000072765.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
            </figure>
            <procstep translate="no">
               <para translate="yes">Position the splined socket<markref format="dita" href="#d1e159/10-02-123-repair-32-02-01-01-01-43-1-1-25" scope="local" translate="no" type="figure">8</markref>from the kit<toolref format="dita" href="x-wc://file=OS-000017226.xml" scope="external" translate="no" type="specifictool">
                     <linkdesc>Kit for replacing the alternator idler pulleys.</linkdesc>
                     <linkvalue>Mot. 1732</linkvalue>
                  </toolref>in the free wheel pulley<markref format="dita" href="#d1e159/10-02-123-repair-32-02-01-01-01-43-1-1-25" scope="local" translate="no" type="figure">7</markref>.</para>
            </procstep>
         </procsteps>
         <procsteps translate="no">
            <marklayer id="marklayer-d1e242" translate="no">
               <mark id="MARK.REPAIR-32X02X01X01-01X43-1-1.7" translate="no">
                  <mcomponent translate="no">
                     <label translate="no"/>
                  </mcomponent>
                  <designation translate="no">9</designation>
               </mark>
               <mark id="MARK.REPAIR-32X02X01X01-01X43-1-1.8" translate="no">
                  <mcomponent translate="no">
                     <label translate="no"/>
                  </mcomponent>
                  <designation translate="no">8</designation>
               </mark>
            </marklayer>
            <figure id="10-02-124-repair-32-02-01-01-01-43-1-1-26" translate="no">
               
               <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000018465_LAY-000072766.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
            </figure>
            <procstep translate="no">
               <para translate="yes">Position the end piece<markref format="dita" href="#d1e159/10-02-124-repair-32-02-01-01-01-43-1-1-26" scope="local" translate="no" type="figure">9</markref>from the kit<toolref format="dita" href="x-wc://file=OS-000017226.xml" scope="external" translate="no" type="specifictool">
                     <linkdesc>Kit for replacing the alternator idler pulleys.</linkdesc>
                     <linkvalue>Mot. 1732</linkvalue>
                  </toolref>in the splined socket<markref format="dita" href="#d1e159/10-02-124-repair-32-02-01-01-01-43-1-1-26" scope="local" translate="no" type="figure">8</markref>.</para>
            </procstep>
         </procsteps>
         <procsteps translate="no">
            <marklayer id="marklayer-d1e270" translate="no">
               <mark id="MARK.REPAIR-32X02X01X01-01X43-1-1.9" translate="no">
                  <mcomponent translate="no">
                     <label translate="no"/>
                  </mcomponent>
                  <designation translate="no">3</designation>
               </mark>
            </marklayer>
            <figure id="10-02-125-repair-32-02-01-01-01-43-1-1-27" translate="no">
               
               <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000018466_LAY-000072767.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
            </figure>
            <procstep translate="no">
               <para translate="yes">Position the door end piece<markref format="dita" href="#d1e159/10-02-125-repair-32-02-01-01-01-43-1-1-27" scope="local" translate="no" type="figure">3</markref>.</para>
            </procstep>
         </procsteps>
         <procsteps translate="no">
            <marklayer id="marklayer-d1e287" translate="no">
               <mark id="MARK.REPAIR-32X02X01X01-01X43-1-1.10" translate="no">
                  <mcomponent translate="no">
                     <label translate="no"/>
                  </mcomponent>
                  <designation translate="no">4</designation>
               </mark>
               <mark id="MARK.REPAIR-32X02X01X01-01X43-1-1.11" translate="no">
                  <mcomponent translate="no">
                     <label translate="no"/>
                  </mcomponent>
                  <designation translate="no">6</designation>
               </mark>
               <mark id="MARK.REPAIR-32X02X01X01-01X43-1-1.12" translate="no">
                  <mcomponent translate="no">
                     <label translate="no"/>
                  </mcomponent>
                  <designation translate="no">8</designation>
               </mark>
            </marklayer>
            <figure id="10-02-126-repair-32-02-01-01-01-43-1-1-28" translate="no">
               
               <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000018467_LAY-000072768.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
            </figure>
            <procstep translate="no">
               <para translate="yes">Hold the splined socket<markref format="dita" href="#d1e159/10-02-126-repair-32-02-01-01-01-43-1-1-28" scope="local" translate="no" type="figure">8</markref>using an open-jawed spanner<markref format="dita" href="#d1e159/10-02-126-repair-32-02-01-01-01-43-1-1-28" scope="local" translate="no" type="figure">4</markref>.</para>
            </procstep>
            <procstep translate="no">
               <para translate="yes">Tighten the alternator free wheel pulley using a ratchet<markref format="dita" href="#d1e159/10-02-126-repair-32-02-01-01-01-43-1-1-28" scope="local" translate="no" type="figure">6</markref>(turning the ratchet anti-clockwise).</para>
            </procstep>
         </procsteps>
         <procsteps translate="no">
            <marklayer id="marklayer-d1e325" translate="no">
               <mark id="MARK.REPAIR-32X02X01X01-01X43-1-1.13" translate="no">
                  <mcomponent translate="no">
                     <label translate="no"/>
                  </mcomponent>
                  <designation translate="no">10</designation>
               </mark>
            </marklayer>
            <figure id="10-02-129-repair-32-02-01-01-01-43-1-1-29" translate="no">
               
               <image dimension="F1 (85x60) #" href="../MergedPictures/ILL-000018470_LAY-000072769.png" style="width: 85.0mm; height: 60.0mm;" translate="no"/>
            </figure>
            <procstep translate="no">
               <noteR translate="no" type="note">
                  <para translate="yes">Note:</para>
                  <para translate="yes">Check that the torque wrench can be used to tighten anti-clockwise.</para>
               </noteR>
            </procstep>
            <procstep translate="no">
               <para translate="yes">Torque tighten the<torque translate="no">
                     <torquedobject translate="yes">alternator free wheel pulley</torquedobject>
                     <technicalvalue translate="no">
                        <value translate="yes">80 Nm</value>
                     </technicalvalue>
                  </torque>using a torque wrench<markref format="dita" href="#d1e159/10-02-129-repair-32-02-01-01-01-43-1-1-29" scope="local" translate="no" type="figure">10</markref>.</para>
            </procstep>
            <procstep translate="no">
               <para translate="yes">Refit the alternator<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3451714">
                     <linkdesc>alternator : removal - refitting</linkdesc>
                     <linkvalue>GIEMR-000049</linkvalue>
                  </sieref>.</para>
            </procstep>
         </procsteps>
      </procbody>
   </procedure>
</procedure>