<?xml version="1.0" encoding="UTF-8" standalone="no"?><procedure xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" appl="X35/CA,CAREG|X65/CA,CAREG" id="RPR-000235572" xml:lang="en" xsi:noNamespaceSchemaLocation="urn:ptc:names:renault:dita:xsd:procedure.xsd">
   <title translate="yes">cabin filter : removal - refitting</title>
   
   
   <procprolog translate="no">
      
      
      <status name="" translate="no"/>
      <critdates translate="no">
         <created date="" translate="no"/>
         <revised modified="" translate="no"/>
      </critdates>
      <procmetadata translate="no">
         <siename translate="no"/>
         <sieversion translate="no"/>
         <sietype translate="yes">Procedure</sietype>
         <sienumber translate="no"/>
         <domain translate="no"/>
         <company translate="no">RENAULT</company>
         
      </procmetadata>
   </procprolog>
   <procbody translate="no"/>
   <procedure id="d1e14" otherprops="WF-SET='01,34'" translate="no">
      <title translate="yes">removal</title>
      <procbody translate="no"/>
      <procedure appl="X35/CA,CAREG" id="d1e18" translate="no">
         <title translate="yes">removal preparation operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove:</para>
                  <ul translate="no">
                     <li translate="yes">the windscreen wiper arms<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3477507">
                           <linkdesc>windscreen wiper arm : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000373</linkvalue>
                        </sieref>(85A, Wiping - Washing),</li>
                     <li translate="yes">the scuttle panel grille<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3483343">
                           <linkdesc>scuttle panel grille : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001380</linkvalue>
                        </sieref>(56A, Exterior equipment).</li>
                  </ul>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure appl="X65/CA,CAREG" id="d1e36" translate="no">
         <title translate="yes">removal preparation operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove the right-hand scuttle panel grille<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3483343">
                        <linkdesc>scuttle panel grille : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-001380</linkvalue>
                     </sieref>(56A, Exterior equipment).</para>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e49" translate="no">
         <title translate="yes">removal operation</title>
         <procbody translate="no">
            <procsteps appl="X35/CAREG|X65/CAREG" translate="no">
               <procstep translate="no">
                  <para translate="yes">Separate the electrical wiring from the cabin filter access flap.</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <marklayer id="marklayer-d1e65" translate="no">
                  <mark id="MARK.REPAIR-30X02X01X14-01X37-1-22.1" translate="no">
                     <mcomponent translate="no">
                        <label translate="no"/>
                     </mcomponent>
                     <designation translate="no">1</designation>
                  </mark>
               </marklayer>
               <figure id="30-02-01-14-58-repair-30-02-01-14-01-37-1-22-1" translate="no">
                  
                  <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000033704_LAY-000058662.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
               </figure>
               <procstep translate="no">
                  <para translate="yes">Remove:</para>
                  <ul translate="no">
                     <li translate="yes">the bolt<markref format="dita" href="#d1e49/30-02-01-14-58-repair-30-02-01-14-01-37-1-22-1" scope="local" translate="no" type="figure">1</markref>from the cabin filter access flap,</li>
                     <li translate="yes">the cabin filter access flap.</li>
                  </ul>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Remove the cabin filter.</para>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
   </procedure>
   <procedure id="d1e91" otherprops="WF-SET='01,61'" translate="no">
      <title translate="yes">refitting</title>
      <procbody translate="no"/>
      <procedure id="d1e95" translate="no">
         <title translate="yes">refitting preparation operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Check there are no foreign bodies in the passenger compartment filter housing and clean thoroughly if necessary.</para>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e102" translate="no">
         <title translate="yes">refitting operation</title>
         <procbody translate="no">
            <procsteps appl="X65/CA,CAREG" translate="no">
               <marklayer id="marklayer-d1e104" translate="no">
                  <mark id="MARK.REPAIR-30X02X01X14-01X37-1-22.2" translate="no">
                     <mcomponent translate="no">
                        <label translate="no"/>
                     </mcomponent>
                     <designation translate="no">2</designation>
                  </mark>
               </marklayer>
               <figure id="30-02-01-14-19-repair-30-02-01-14-01-37-1-22-2" translate="no">
                  
                  <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000017675_LAY-000058663.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
               </figure>
               <procstep translate="no">
                  <noteR translate="no" type="note">
                     <para translate="yes">Note:</para>
                     <para translate="yes">Be sure to fit the particle filter the right way round (the two arrows<markref format="dita" href="#d1e102/30-02-01-14-19-repair-30-02-01-14-01-37-1-22-2" scope="local" translate="no" type="figure">2</markref>in the same direction).</para>
                  </noteR>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit:</para>
                  <ul translate="no">
                     <li translate="yes">the cabin filter,</li>
                     <li translate="yes">the cabin filter access flap.</li>
                  </ul>
               </procstep>
            </procsteps>
            <procsteps appl="X35/CAREG|X65/CAREG" translate="no">
               <procstep translate="no">
                  <para translate="yes">Fit the electrical wiring onto the cabin filter access flap.</para>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure appl="X65/CA,CAREG" id="d1e152" translate="no">
         <title translate="yes">final operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit the right-hand scuttle panel grille<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3483343">
                        <linkdesc>scuttle panel grille : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-001380</linkvalue>
                     </sieref>(56A, Exterior equipment).</para>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure appl="X35/CA,CAREG" id="d1e162" translate="no">
         <title translate="yes">final operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit:</para>
                  <ul translate="no">
                     <li translate="yes">the scuttle panel grille<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3483343">
                           <linkdesc>scuttle panel grille : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001380</linkvalue>
                        </sieref>(56A, Exterior equipment).</li>
                     <li translate="yes">the windscreen wiper arms<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3477507">
                           <linkdesc>windscreen wiper arm : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000373</linkvalue>
                        </sieref>(85A, Wiping - Washing).</li>
                  </ul>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
   </procedure>
</procedure>