<?xml version="1.0" encoding="UTF-8" standalone="no"?><procedure xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" appl="X35/BVM5|X65/BVM5" id="RPR-000268581" xml:lang="en" xsi:noNamespaceSchemaLocation="urn:ptc:names:renault:dita:xsd:procedure.xsd">
   <title translate="yes">clutch pedal : removal - refitting</title>
   
   
   <procprolog translate="no">
      
      
      <status name="" translate="no"/>
      <critdates translate="no">
         <created date="" translate="no"/>
         <revised modified="" translate="no"/>
      </critdates>
      <procmetadata translate="no">
         <siename translate="no"/>
         <sieversion translate="no"/>
         <sietype translate="yes">Procedure</sietype>
         <sienumber translate="no"/>
         <domain translate="no"/>
