<?xml version="1.0" encoding="UTF-8" standalone="no"?><procedure xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" appl="X35|X65/JB1" id="RPR-000261178" xml:lang="en" xsi:noNamespaceSchemaLocation="urn:ptc:names:renault:dita:xsd:procedure.xsd">
   <title translate="yes">pedal assembly bracket : removal - refitting</title>
   
   
   <procprolog translate="no">
      
      
      <status name="" translate="no"/>
      <critdates translate="no">
         <created date="" translate="no"/>
         <revised modified="" translate="no"/>
      </critdates>
      <procmetadata translate="no">
         <siename translate="no"/>
         <sieversion translate="no"/>
         <sietype translate="yes">Procedure</sietype>
         <sienumber translate="no"/>
         <domain translate="no"/>
         <company translate="no">RENAULT</company>
         
      </procmetadata>
   </procprolog>
   <procbody translate="no"/>
   <procedure id="d1e10" otherprops="WF-SET='01,34'" translate="no">
      <title translate="yes">removal</title>
      <procbody translate="no"/>
      <procedure id="d1e14" translate="no">
         <title translate="yes">removal preparation operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Disconnect the battery<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3511330">
                        <linkdesc>12 V battery : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-000248</linkvalue>
                     </sieref>(80A, Battery).</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove:</para>
                  <ul translate="no">
                     <li translate="yes">the brake pedal switch<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3516657">
                           <linkdesc>brake pedal switch : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000697</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the steering column<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3472147">
                           <linkdesc>steering column : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000566</linkvalue>
                        </sieref>.</li>
                  </ul>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e38" translate="no">
         <title translate="yes">removal operation</title>
         <procbody translate="no">
            <procsteps appl="X35/JB1,JB3|X65/JB1" translate="no">
               <figure id="30-03-01-02-18-repair-30-03-07-01-37-1-1-15" translate="no">
                  
                  <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000006155_LAY-000068633.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
               </figure>
               <procstep translate="no">
                  <para translate="yes">Remove the cable from the clutch pedal housing, by lifting the clutch pedal and placing the cable perpendicular to the clutch pedal.</para>
               </procstep>
            </procsteps>
            <procsteps appl="X35/JH3,JR5" translate="no">
               <marklayer id="marklayer-d1e55" translate="no">
                  <mark id="MARK.REPAIR-30X03X07-01X37-1-1.1" translate="no">
                     <mcomponent translate="no">
                        <label translate="no"/>
                     </mcomponent>
                     <designation translate="no">1</designation>
                  </mark>
               </marklayer>
               <figure id="12-04-02-02-21-repair-30-03-07-01-37-1-1-16" translate="no">
                  
                  <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000031680_LAY-000068634.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
               </figure>
               <procstep translate="no">
                  <para translate="yes">Unclip the clutch master cylinder ball joint<markref format="dita" href="#d1e38/12-04-02-02-21-repair-30-03-07-01-37-1-1-16" scope="local" translate="no" type="figure">1</markref>.</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <marklayer id="marklayer-d1e78" translate="no">
                  <mark id="MARK.REPAIR-30X03X07-01X37-1-1.2" translate="no">
                     <mcomponent translate="no">
                        <label translate="no"/>
                     </mcomponent>
                     <designation translate="no">2</designation>
                  </mark>
               </marklayer>
               <figure id="30-03-02-01-50-repair-30-03-07-01-37-1-1-17" translate="no">
                  
                  <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000045151_LAY-000068635.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
               </figure>
               <procstep translate="no">
                  <para translate="yes">Remove:</para>
                  <ul translate="no">
                     <li translate="yes">the clip<markref format="dita" href="#d1e38/30-03-02-01-50-repair-30-03-07-01-37-1-1-17" scope="local" translate="no" type="figure">2</markref>on the brake servo shaft,</li>
                     <li translate="yes">the brake servo connecting shaft.</li>
                  </ul>
                  <para translate="yes"/>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <marklayer id="marklayer-d1e101" translate="no">
                  <mark id="MARK.REPAIR-30X03X07-01X37-1-1.3" translate="no">
                     <mcomponent translate="no">
                        <label translate="no"/>
                     </mcomponent>
                     <designation translate="no">4</designation>
                  </mark>
                  <mark id="MARK.REPAIR-30X03X07-01X37-1-1.4" translate="no">
                     <mcomponent translate="no">
                        <label translate="no"/>
                     </mcomponent>
                     <designation translate="no">3</designation>
                  </mark>
               </marklayer>
               <figure id="30-03-04-02-7-repair-30-03-07-01-37-1-1-13" translate="no">
                  
                  <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000045153_LAY-000068631.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
               </figure>
               <procstep translate="no">
                  <para translate="yes">Remove:</para>
                  <ul translate="no">
                     <li translate="yes">the nuts<markref format="dita" href="#d1e38/30-03-04-02-7-repair-30-03-07-01-37-1-1-13" scope="local" translate="no" type="figure">3</markref>of the pedal soundproofing,</li>
                     <li translate="yes">the pedal assembly soundproofing<markref format="dita" href="#d1e38/30-03-04-02-7-repair-30-03-07-01-37-1-1-13" scope="local" translate="no" type="figure">4</markref>.</li>
                  </ul>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <marklayer id="marklayer-d1e134" translate="no">
                  <mark id="MARK.REPAIR-30X03X07-01X37-1-1.5" translate="no">
                     <mcomponent translate="no">
                        <label translate="no"/>
                     </mcomponent>
                     <designation translate="no">5</designation>
                  </mark>
               </marklayer>
               <figure id="30-03-02-01-48-repair-30-03-07-01-37-1-1-14" translate="no">
                  
                  <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000045149_LAY-000068632.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
               </figure>
               <procstep translate="no">
                  <para translate="yes">Remove:</para>
                  <ul translate="no">
                     <li translate="yes">the brake servo nuts<markref format="dita" href="#d1e38/30-03-02-01-48-repair-30-03-07-01-37-1-1-14" scope="local" translate="no" type="figure">5</markref>,</li>
                     <li translate="yes">the pedal yoke.</li>
                  </ul>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">In the event of replacement, remove:</para>
                  <ul translate="no">
                     <li translate="yes">the clutch pedal<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3523841">
                           <linkdesc>clutch pedal : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001849</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the brake pedal<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3533558">
                           <linkdesc>brake pedal : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001850</linkvalue>
                        </sieref>.</li>
                  </ul>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
   </procedure>
   <procedure id="d1e175" otherprops="WF-SET='01,61'" translate="no">
      <title translate="yes">refitting</title>
      <procbody translate="no"/>
      <procedure id="d1e179" translate="no">
         <title translate="yes">refitting operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">In the event of replacement, refit:</para>
                  <ul translate="no">
                     <li translate="yes">the brake pedal<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3533558">
                           <linkdesc>brake pedal : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001850</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the clutch pedal<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3523841">
                           <linkdesc>clutch pedal : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-001849</linkvalue>
                        </sieref>.</li>
                  </ul>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Refit the pedal yoke.</para>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Torque tighten the<torque translate="no">
                        <torquedobject translate="yes">brake servo nuts</torquedobject>
                        <technicalvalue translate="no">
                           <value translate="yes">24 N.m</value>
                        </technicalvalue>
                     </torque>.</para>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Refit the soundproofing material.</para>
               </procstep>
               <procstep translate="no">
                  <para translate="yes">Refit:</para>
                  <ul translate="no">
                     <li translate="yes">the brake servo connecting shaft,</li>
                     <li translate="yes">the clip on the brake servo shaft.</li>
                  </ul>
                  <para translate="yes"/>
               </procstep>
            </procsteps>
            <procsteps appl="X35/JB1,JB3|X65/JB1" translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit the clutch cable in the notch of the clutch pedal.</para>
               </procstep>
            </procsteps>
            <procsteps appl="X35/JH3,JR5" translate="no">
               <procstep translate="no">
                  <para translate="yes">Clip on the clutch master cylinder ball joint.</para>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e247" translate="no">
         <title translate="yes">final operation</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Refit:</para>
                  <ul translate="no">
                     <li translate="yes">the steering column<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3472147">
                           <linkdesc>steering column : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000566</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the brake pedal switch<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3516657">
                           <linkdesc>brake pedal switch : removal - refitting</linkdesc>
                           <linkvalue>GIEMR-000697</linkvalue>
                        </sieref>.</li>
                  </ul>
                  <para translate="yes"/>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Connect the battery<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3511330">
                        <linkdesc>12 V battery : removal - refitting</linkdesc>
                        <linkvalue>GIEMR-000248</linkvalue>
                     </sieref>(80A, Battery).</para>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
   </procedure>
</procedure>