<?xml version="1.0" encoding="UTF-8" standalone="no"?><procedure xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" appl="X35|X65" id="RPR-000223045" xml:lang="en" xsi:noNamespaceSchemaLocation="urn:ptc:names:renault:dita:xsd:procedure.xsd"><title translate="yes">front wheel speed sensor : removal - refitting</title><procprolog translate="no"><status name="" translate="no"/><critdates translate="no"><created date="" translate="no"/><revised modified="" translate="no"/></critdates><procmetadata translate="no"><siename translate="no"/><sieversion translate="no"/><sietype translate="yes">Procedure</sietype><sienumber translate="no"/><domain translate="no"/><company translate="no">RENAULT</company></procmetadata></procprolog><procbody translate="no"/><procedure id="d1e8" otherprops="WF-SET='01,34'" translate="no"><title translate="yes">removal</title><procbody translate="no"/><procedure id="d1e12" translate="no"><title translate="yes">removal preparation operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Position the vehicle on a two-post lift<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3500084"><linkdesc>vehicle : towing and lifting</linkdesc><linkvalue>GIEMR-002447</linkvalue></sieref>(02A, Lifting equipment).</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Remove:</para><ul translate="no"><li translate="yes">the front wheel<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3532435"><linkdesc>wheel : removal - refitting</linkdesc><linkvalue>GIEMR-001965</linkvalue></sieref>,</li><li translate="yes">the front wheel arch liner<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3485753"><linkdesc>front wheel arch liner : removal - refitting</linkdesc><linkvalue>GIEMR-002352</linkvalue></sieref>(55A, Exterior protection).</li></ul></procstep></procsteps></procbody></procedure><procedure id="d1e36" translate="no"><title translate="yes">removal operation</title><procbody translate="no"><procsteps translate="no"><marklayer id="marklayer-d1e38" translate="no"><mark id="MARK.REPAIR-13X03X10X14-01X37-1-7.1" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">2</designation></mark><mark id="MARK.REPAIR-13X03X10X14-01X37-1-7.2" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">1</designation></mark></marklayer><figure id="13-03-10-14-12-repair-13-03-10-14-01-37-1-7-5" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000043360_LAY-000055433.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Remove the wheel speed sensor bolt<markref format="dita" href="#d1e36/13-03-10-14-12-repair-13-03-10-14-01-37-1-7-5" scope="local" translate="no" type="figure">1</markref>.</para></procstep><procstep translate="no"><para translate="yes">Unpick the wheel speed sensor wiring at<markref format="dita" href="#d1e36/13-03-10-14-12-repair-13-03-10-14-01-37-1-7-5" scope="local" translate="no" type="figure">2</markref>.</para></procstep></procsteps><procsteps translate="no"><marklayer id="marklayer-d1e67" translate="no"><mark id="MARK.REPAIR-13X03X10X14-01X37-1-7.3" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">4</designation></mark><mark id="MARK.REPAIR-13X03X10X14-01X37-1-7.4" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">3</designation></mark></marklayer><figure id="13-03-10-14-11-repair-13-03-10-14-01-37-1-7-6" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000043359_LAY-000055434.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Unclip the connector<markref format="dita" href="#d1e36/13-03-10-14-11-repair-13-03-10-14-01-37-1-7-6" scope="local" translate="no" type="figure">3</markref>from the wheel speed sensor wiring at<markref format="dita" href="#d1e36/13-03-10-14-11-repair-13-03-10-14-01-37-1-7-6" scope="local" translate="no" type="figure">4</markref>.</para></procstep><procstep translate="no"><para translate="yes">Disconnect the wheel speed sensor connector.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Remove the wheel speed sensor.</para></procstep></procsteps></procbody></procedure></procedure><procedure id="d1e100" otherprops="WF-SET='01,61'" translate="no"><title translate="yes">refitting</title><procbody translate="no"/><procedure id="d1e104" translate="no"><title translate="yes">refitting operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><noteR translate="no" type="caution"><para translate="yes">To avoid damaging the wheel speed sensor cable:</para><ul translate="no"><li translate="yes">Do not tension the cable,</li><li translate="yes">Do not twist the cable,</li><li translate="yes">Check that there is no contact with the surrounding components,</li><li translate="yes">Do not use tools that may damage the cable.</li></ul></noteR></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Refit the wheel speed sensor.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Torque tighten the wheel speed sensor bolt<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3540366"><linkdesc>brake circuit : tightening torque</linkdesc><linkvalue>GIEMR-000520</linkvalue></sieref>.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Connect the wheel speed sensor connector.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Clip:</para><ul translate="no"><li translate="yes">the wheel speed sensor connector,</li><li translate="yes">the wheel speed sensor wiring,</li><li translate="yes">the wheel speed sensor.</li></ul></procstep></procsteps></procbody></procedure><procedure id="d1e143" translate="no"><title translate="yes">final operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Refit:</para><ul translate="no"><li translate="yes">the front wheel arch liner<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3485753"><linkdesc>front wheel arch liner : removal - refitting</linkdesc><linkvalue>GIEMR-002352</linkvalue></sieref>(55A, Exterior protection),</li><li translate="yes">the front wheel<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3532435"><linkdesc>wheel : removal - refitting</linkdesc><linkvalue>GIEMR-001965</linkvalue></sieref>.</li></ul></procstep></procsteps></procbody></procedure></procedure></procedure>