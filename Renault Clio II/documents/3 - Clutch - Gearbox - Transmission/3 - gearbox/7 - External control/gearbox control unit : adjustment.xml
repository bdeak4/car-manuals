<?xml version="1.0" encoding="UTF-8" standalone="no"?><!--Arbortext, Inc., 1988-2018, v.4002--><procedure xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" appl="X65/JB1,JB3" id="RPR-000188368" xml:lang="en" xsi:noNamespaceSchemaLocation="urn:ptc:names:renault:dita:xsd:procedure.xsd"><title translate="yes">gearbox control unit : adjustment</title><procprolog translate="no"><status name="Validated" translate="no"/><critdates translate="no"><created date="28/01/22 11:13" translate="no"/><revised modified="28/01/22 11:14" translate="no"/></critdates><procmetadata translate="no"><siename translate="no">gearbox control unit : adjustment</siename><sieversion translate="no">C.2</sieversion><sietype translate="yes">Procedure</sietype><sienumber translate="no">RPR-000188368</sienumber><domain translate="no">MR</domain><company translate="no">RENAULT</company></procmetadata></procprolog><procedure id="d1e9" otherprops="WF-SET='01,67'" translate="no"><title translate="yes">adjustment</title><procedure id="d1e13" translate="no"><title translate="yes">adjustment preparation operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Position the vehicle on a two-post lift<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3500084"><linkdesc>vehicle : towing and lifting</linkdesc><linkvalue>GIEMR-002447</linkvalue></sieref>.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Engage first gear.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Remove the engine undertray.</para></procstep></procsteps><procsteps appl="X65/JB1,JB3/PE2005" translate="no"><figure id="12-02-01-03-21-repair-12-10-01-67-1-3-12" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000017886_LAY-000051666.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/><illusnum>RPR-000188368-02-01-03-21-repair-12-10-01-67-1-3-12-000017886</illusnum></figure><procstep translate="no"><para translate="yes">Remove the heat shield flap to gain access to the gear lever.</para></procstep></procsteps><procsteps appl="X65/JB1/PE2009" translate="no"><marklayer id="marklayer-d1e43" translate="no"><mark id="MARK.REPAIR-12X10-01X67-1-3.8" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">1</designation></mark></marklayer><figure id="12-01-649-repair-12-10-01-67-1-3-16" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000055151_LAY-000051669.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/><illusnum>RPR-000188368-01-649-repair-12-10-01-67-1-3-16-000055151</illusnum></figure><procstep translate="no"><para translate="yes">Remove:</para><ul translate="no"><li translate="yes">the bolts<markref format="dita" href="#d1e13/12-01-649-repair-12-10-01-67-1-3-16" scope="local" translate="no" type="figure">1</markref> from the heat shield,</li><li translate="yes">the heat shield to gain access to the gear lever.</li></ul></procstep></procsteps></procbody></procedure><procedure id="d1e77" translate="no"><title translate="yes">adjustment operation</title><procbody translate="no"><procsteps translate="no"><marklayer id="marklayer-d1e79" translate="no"><mark id="MARK.REPAIR-12X10-01X67-1-3.10" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">2</designation></mark></marklayer><figure id="12-02-01-03-51-repair-12-10-01-67-1-3-17" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000028481_LAY-000051670.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/><illusnum>RPR-000188368-02-01-03-51-repair-12-10-01-67-1-3-17-000028481</illusnum></figure><procstep translate="no"><para translate="yes">Loosen the linkage adjustment nut<markref format="dita" href="#d1e77/12-02-01-03-51-repair-12-10-01-67-1-3-17" scope="local" translate="no" type="figure">2</markref>.</para></procstep></procsteps><procsteps translate="no"><marklayer id="marklayer-d1e97" translate="no"><mark id="MARK.REPAIR-12X10-01X67-1-3.1" translate="no"><mcomponent translate="no"><label translate="no">TOOL-REF:1000916</label></mcomponent><designation translate="no">3</designation></mark><mark id="MARK.REPAIR-12X10-01X67-1-3.11" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">4</designation></mark></marklayer><figure id="fig1" translate="no"><image dimension="F1 (85x60) #" href="../MergedPictures/ILL-000065642.PNG" style="width: 85.0mm; height: 60.0mm;" translate="no"/><illusnum>RPR-000188368-01-000065642</illusnum></figure><figure id="13-02-01-47-repair-12-10-01-67-1-3-14" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000017897_LAY-000051668.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/><illusnum>RPR-000188368-02-01-47-repair-12-10-01-67-1-3-14-000017897</illusnum></figure><procstep translate="no"><para translate="yes">Fit the tool<markref format="dita" href="#d1e77/13-02-01-47-repair-12-10-01-67-1-3-14" scope="local" translate="no" type="figure">3</markref><toolref format="dita" href="x-wc://file=OS-000005533.xml" scope="external" translate="no" type="specifictool"><linkdesc>1st gear locking shim for gearbox input lever.</linkdesc><linkvalue>Bvi.1133-01</linkvalue></toolref> between the gear control lever<markref format="dita" href="#d1e77/13-02-01-47-repair-12-10-01-67-1-3-14" scope="local" translate="no" type="figure">4</markref> and the gearbox casing to compensate for the operating clearances.</para></procstep></procsteps><procsteps translate="no"><marklayer id="marklayer-d1e127" translate="no"><mark id="MARK.REPAIR-12X10-01X67-1-3.15" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">Y</designation></mark></marklayer><figure id="12-10-105-repair-12-10-01-67-1-3-18" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000054615_LAY-000051671.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/><illusnum>RPR-000188368-10-105-repair-12-10-01-67-1-3-18-000054615</illusnum><?Pub Caret -1?></figure><procstep translate="no"><para translate="yes">Position a <technicalvalue translate="no"><value translate="yes">9 mm</value></technicalvalue> thick permanent shim between the lower thrust bearing of the gear lever and the lower rail of the unit<markref format="dita" href="#d1e77/12-10-105-repair-12-10-01-67-1-3-18" scope="local" translate="no" type="figure">Y</markref>.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Hold the gear control lever against the shim.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Torque tighten the<torque translate="no"><torquedobject translate="yes">linkage adjustment nut</torquedobject><technicalvalue translate="no"><value translate="yes">28 N.m</value></technicalvalue></torque>.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Check the adjustment value.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Clearance noted at the bottom of the lever<markref format="dita" href="#d1e77/12-10-105-repair-12-10-01-67-1-3-18" scope="local" translate="no" type="figure">Y</markref>(in mm):</para><ul translate="no"><li translate="yes">first gear engaged =<technicalvalue translate="no"><value translate="yes">Y1</value></technicalvalue>,</li><li translate="yes">second gear engaged =<technicalvalue translate="no"><value translate="yes">Y2</value></technicalvalue>,</li></ul><para translate="yes"><technicalvalue translate="no"><value translate="yes">7 ≤ (Y1+Y2)/2 ≤ 10</value></technicalvalue></para><noteR translate="no" type="note"><para translate="yes">Note:</para><para translate="yes">It is preferable to be closer to the maximum value of the adjustment interval.</para></noteR></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Remove:</para><ul translate="no"><li translate="yes">the tool <toolref format="dita" href="x-wc://file=OS-000005533.xml" scope="external" translate="no" type="specifictool"><linkdesc>1st gear locking shim for gearbox input lever.</linkdesc><linkvalue>Bvi.1133-01</linkvalue></toolref>,</li><li translate="yes">the permanent shim.</li></ul></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Check gear changes.</para></procstep></procsteps></procbody></procedure></procedure><procedure id="pro1" translate="no"><title translate="yes">final operation</title><procbody translate="no"><procsteps appl="X65/JB1,JB3/PE2005" translate="no"><procstep translate="no"><para translate="yes">Refit the heat shield flap.</para></procstep></procsteps><procsteps appl="X65/JB1/PE2009" translate="no"><procstep translate="no"><para translate="yes">Refit the heat shield.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Refit the engine undertray.</para></procstep></procsteps></procbody></procedure></procedure>