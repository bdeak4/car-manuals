<?xml version="1.0" encoding="UTF-8" standalone="no"?><procedure xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" appl="X65/JB1,JB3,JC5,JH1,PK6" id="RPR-000171169" xml:lang="en" xsi:noNamespaceSchemaLocation="urn:ptc:names:renault:dita:xsd:procedure.xsd"><title translate="yes">gear control assembly : removal - refitting</title><procprolog translate="no"><status name="" translate="no"/><critdates translate="no"><created date="" translate="no"/><revised modified="" translate="no"/></critdates><procmetadata translate="no"><siename translate="no"/><sieversion translate="no"/><sietype translate="yes">Procedure</sietype><sienumber translate="no"/><domain translate="no"/><company translate="no">RENAULT</company></procmetadata></procprolog><procbody translate="no"/><procedure id="d1e12" otherprops="WF-SET='01,34'" translate="no"><title translate="yes">removal</title><procbody translate="no"/><procedure id="d1e16" translate="no"><title translate="yes">Removal preparation operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Position the vehicle on a two-post lift<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3500084"><linkdesc>vehicle : towing and lifting</linkdesc><linkvalue>GIEMR-002447</linkvalue></sieref>(02A, Lifting equipment).</para></procstep></procsteps><procsteps translate="no"><marklayer id="marklayer-d1e24" translate="no"><mark id="MARK.REPAIR-12X02X01X03-01X37-1-8.1" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">1</designation></mark></marklayer><figure id="12-01-97-repair-12-02-01-03-01-37-1-8-1" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000012117_LAY-000049737.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Detach the gaiter<markref format="dita" href="#d1e16/12-01-97-repair-12-02-01-03-01-37-1-8-1" scope="local" translate="no" type="figure">1</markref>inside the vehicle.</para></procstep></procsteps><procsteps translate="no"><marklayer id="marklayer-d1e42" translate="no"><mark id="MARK.REPAIR-12X02X01X03-01X37-1-8.2" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">3</designation></mark><mark id="MARK.REPAIR-12X02X01X03-01X37-1-8.3" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">2</designation></mark></marklayer><figure id="10-01-27-repair-12-02-01-03-01-37-1-8-2" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000012214_LAY-000049738.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Remove the exhaust pipe clip<markref format="dita" href="#d1e16/10-01-27-repair-12-02-01-03-01-37-1-8-2" scope="local" translate="no" type="figure">2</markref>.</para></procstep><procstep translate="no"><para translate="yes">Disconnect the exhaust.</para></procstep><procstep translate="no"><para translate="yes">Remove:</para><ul translate="no"><li translate="yes">the clips or nuts of the heat shields,</li><li translate="yes">the heat shields<markref format="dita" href="#d1e16/10-01-27-repair-12-02-01-03-01-37-1-8-2" scope="local" translate="no" type="figure">3</markref>.</li></ul></procstep></procsteps></procbody></procedure><procedure id="d1e82" translate="no"><title translate="yes">Removal operation</title><procbody translate="no"><procsteps translate="no"><marklayer id="marklayer-d1e84" translate="no"><mark id="MARK.REPAIR-12X02X01X03-01X37-1-8.4" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">5</designation></mark><mark id="MARK.REPAIR-12X02X01X03-01X37-1-8.5" translate="no"><mcomponent translate="no"><label translate="no"/></mcomponent><designation translate="no">4</designation></mark></marklayer><figure id="12-02-01-03-21-repair-12-02-01-03-01-37-1-8-3" translate="no"><image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000017886_LAY-000049739.png" style="width: 85.0mm; height: 85.0mm;" translate="no"/></figure><procstep translate="no"><para translate="yes">Remove:</para><ul translate="no"><li translate="yes">the nut<markref format="dita" href="#d1e82/12-02-01-03-21-repair-12-02-01-03-01-37-1-8-3" scope="local" translate="no" type="figure">4</markref>of the gearbox control linkage on the control lever,</li><li translate="yes">the gearbox control linkage,</li><li translate="yes">the gear control unit nuts<markref format="dita" href="#d1e82/12-02-01-03-21-repair-12-02-01-03-01-37-1-8-3" scope="local" translate="no" type="figure">5</markref>,</li><li translate="yes">the gear control unit.</li></ul></procstep></procsteps></procbody></procedure></procedure><procedure id="d1e122" otherprops="WF-SET='01,61'" translate="no"><title translate="yes">refitting</title><procbody translate="no"/><procedure id="d1e126" translate="no"><title translate="yes">refitting preparation operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Coat with<ingredientname translate="yes">SILICONE GREASE</ingredientname><sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3542379"><linkdesc>vehicle : parts and consumables for the repair</linkdesc><linkvalue>GIEMR-002445</linkvalue></sieref>(04B, Consumables - Products) the lever joints and linkage pin.</para></procstep></procsteps></procbody></procedure><procedure id="d1e138" translate="no"><title translate="yes">Refitting operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Refit:</para><ul translate="no"><li translate="yes">the gear control unit,</li><li translate="yes">the gearbox control linkage.</li></ul></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Torque tighten:</para><ul translate="no"><li translate="yes">the<torque translate="no"><torquedobject translate="yes">gear control unit nuts</torquedobject><technicalvalue translate="no"><value translate="yes">15 N.m</value></technicalvalue></torque>,</li><li translate="yes">the<torque translate="no"><torquedobject translate="yes">gear control linkage mounting bolts</torquedobject><technicalvalue translate="no"><value translate="yes">28 N.m</value></technicalvalue></torque>.</li></ul></procstep></procsteps></procbody></procedure><procedure id="d1e174" translate="no"><title translate="yes">Final operation</title><procbody translate="no"><procsteps translate="no"><procstep translate="no"><para translate="yes">Refit:</para><ul translate="no"><li translate="yes">the heat shields,</li><li translate="yes">the clips or nuts of the heat shields,</li></ul></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Couple the exhaust.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Refit the exhaust pipe clip.</para></procstep></procsteps><procsteps translate="no"><procstep translate="no"><para translate="yes">Clip the gaiter on the centre console.</para></procstep></procsteps></procbody></procedure></procedure></procedure>