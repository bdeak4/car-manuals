  <procedure id="d1e87" translate="no">
    <title translate="yes">To carry out fault finding on a heating and air conditioning system circuit:</title>
    <procbody translate="no">
      <procsteps translate="no">
        <figure id="30-02-02-27-1-repair-30-02-02-38-01-79-1-1-1" translate="no">
          <title translate="yes">With a calibrated pressure relief valve:</title>
          <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000001000.PNG" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
          <illusnum>RPR-000249237-02-02-27-1-repair-30-02-02-38-01-79-1-1-1-000001000</illusnum>
        </figure>
        <procstep translate="no">
          <para translate="yes">Use the <ingredientname translate="yes">AIRCODIAG </ingredientname>tool to carry out fault finding on:</para>
          <ul translate="no">
            <li translate="yes">loss of refrigerant pressure or to check the correct pressure of the refrigerant in the circuit.</li>
          </ul>
        </procstep>
        <procstep translate="no">
          <para translate="yes">Refer to the user manual for the device.</para>
        </procstep>
        <procstep translate="no">
          <para translate="yes">Do not use the <ingredientname translate="yes">AIRCODIAG </ingredientname>tool to carry out fault finding on:</para>
          <ul translate="no">
            <li translate="yes">refrigerant fluid circuit overpressure.</li>
            <li translate="yes">a blocked dehydrator filter.</li>
          </ul>
        </procstep>
      </procsteps>
    </procbody>
  </procedure>
  <procedure id="d1e118" translate="no">
    <title translate="yes">To carry out fault finding on a heating and air conditioning system circuit:</title>
    <procbody translate="no">
      <procsteps translate="no">
        <figure id="30-02-02-32-1-repair-30-02-02-38-01-79-1-1-2" translate="no">
          <title translate="yes">With a thermostatic expansion valve:</title>
          <image dimension="F2 (85x85) #" href="../MergedPictures/ILL-000010152.PNG" style="width: 85.0mm; height: 85.0mm;" translate="no"/>
          <illusnum>RPR-000249237-02-02-32-1-repair-30-02-02-38-01-79-1-1-2-000010152</illusnum>
          <?Pub Caret -1?>
        </figure>
        <procstep translate="no">
          <para translate="yes">Use the <ingredientname translate="yes">AIRCODIAG </ingredientname>tool to carry out fault finding on:</para>
          <ul translate="no">
            <li translate="yes">loss of refrigerant pressure or to check the correct pressure of the refrigerant in the circuit.</li>
            <li translate="yes">refrigerant fluid circuit overpressure.</li>
            <li translate="yes">a blocked dehydrator filter.</li>
          </ul>
        </procstep>
        <procstep translate="no">
          <para translate="yes">Refer to the user manual for the device.</para>
        </procstep>
      </procsteps>
    </procbody>
