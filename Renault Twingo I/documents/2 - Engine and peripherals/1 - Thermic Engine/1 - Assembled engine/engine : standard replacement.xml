   <procbody translate="no"/>
   <procedure id="d1e29" translate="no">
      <title translate="yes">standard engine replacement</title>
      <procbody translate="no"/>
      <procedure id="d1e33" translate="no">
         <title translate="yes">Preparation of the old engine for return</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Clean the engine.</para>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Drain:</para>
                  <ul translate="no">
                     <li translate="yes">the engine oil<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3457199">
                           <linkdesc>engine oil : draining - refilling</linkdesc>
                           <linkvalue>GIEMR-001296</linkvalue>
                        </sieref>,</li>
                     <li translate="yes">the coolant<sieref href="x-wc://gie=OR:wt.part.WTPartMaster:3479881">
                           <linkdesc>coolant circuit assembly : draining - refilling</linkdesc>
                           <linkvalue>GIEMR-000954</linkvalue>
                        </sieref>.</li>
                  </ul>
               </procstep>
            </procsteps>
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Secure the old engine to the stand and under the same conditions as for new engine replacement:</para>
                  <ul translate="no">
                     <li translate="yes">fit the plastic plug welds and covers,</li>
                     <li translate="yes">fit the cardboard cover over the whole assembly.</li>
                  </ul>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e62" translate="no">
         <title translate="yes">Parts to leave on the old engine</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Parts to leave on the old engine or to include in the return box:</para>
                  <ul translate="no">
                     <li translate="yes">the dipstick,</li>
                     <li translate="yes">the oil filter,</li>
                     <li translate="yes">the oil level sensor,</li>
                     <li translate="yes">the rocker cover,</li>
                     <li translate="yes">the coolant pump,</li>
                     <li translate="yes">the injector rail,</li>
                     <li translate="yes">the injectors,</li>
                     <li translate="yes">the spark plugs,</li>
                     <li translate="yes">the entire timing end (crankshaft sprocket, belt, tensioner, camshaft sprocket),</li>
                     <li translate="yes">the timing covers,</li>
                     <li translate="yes">the crankshaft accessories pulley,</li>
                     <li translate="yes">the flywheel,</li>
                     <li translate="yes">the clutch pressure plate and disc,</li>
                     <li translate="yes">the lifting rings,</li>
                     <li translate="yes">the cylinder head suspended mounting.</li>
                  </ul>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e101" translate="no">
         <title translate="yes">Parts to remove from the old engine</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Parts to remember to remove from the old engine:</para>
                  <ul translate="no">
                     <li translate="yes">all the cooling system pipes,</li>
                     <li translate="yes">the oil pressure sensor,</li>
                     <li translate="yes">the pinking sensor,</li>
                     <li translate="yes">the coolant temperature sensor,</li>
                     <li translate="yes">the coolant pump inlet pipe,</li>
                     <li translate="yes">the water chamber,</li>
                     <li translate="yes">the inlet distributor,</li>
                     <li translate="yes">the exhaust manifold,</li>
                     <li translate="yes">the alternator,</li>
                     <li translate="yes">the multifunction support.</li>
                  </ul>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e126" translate="no">
         <title translate="yes">Parts to remove from the old engine</title>
         <procbody translate="no">
            <procsteps appl="X06/D7D,D7F/CA,CAREG|X57/D7F/CA,CAREG|X65/D7F/CA,CAREG|X44/D7F/CA,CAREG|X76/D7F/CA,CAREG" translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove the air conditioning compressor from the old engine and refit it on the new engine.</para>
               </procstep>
            </procsteps>
            <procsteps appl="X65/D7D,D7F/DA|X44/D7F/DA|X76/D7F/DA" translate="no">
               <procstep translate="no">
                  <para translate="yes">Remove the power-assisted steering pump from the old engine and refit it on the new engine.</para>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
      <procedure id="d1e185" translate="no">
         <title translate="yes">Parts always to be replaced</title>
         <procbody translate="no">
            <procsteps translate="no">
               <procstep translate="no">
                  <para translate="yes">Always replace:</para>
                  <ul translate="no">
                     <li translate="yes">the water chamber seal on the cylinder head,</li>
                     <li translate="yes">the inlet distributor seal,</li>
                     <li translate="yes">the exhaust manifold gasket,</li>
                     <li translate="yes">the accessories belt(s),</li>
                     <li translate="yes">the accessories belt tensioning roller,</li>
                     <li translate="yes">the accessories belt tensioning roller bolt,</li>
                     <li translate="yes">the cooling circuit hoses if they are damaged.</li>
                  </ul>
               </procstep>
            </procsteps>
         </procbody>
      </procedure>
   </procedure>
   <procedure id="d1e204" translate="no">
      <title translate="yes">standard engine replacement</title>
      <procbody translate="no">
         <procsteps translate="no">
            <procstep translate="no">
               <para translate="yes">Refit the parts removed from the old engine on the new engine.</para>
            </procstep>
         </procsteps>
      </procbody>
   </procedure>
</procedure>